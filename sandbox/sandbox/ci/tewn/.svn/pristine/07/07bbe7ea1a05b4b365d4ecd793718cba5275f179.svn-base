<?php
namespace CI\Demographic\DataSource;

class MyArray extends \CI\Demographic\DataSource
{
    protected $aDemographicFemale = array(
        'id' => \CI\Demographic::DEMOGRAPHIC_SEX_FEMALE,
        'name' => 'Female'
    );

    protected $aDemographicMale = array(
        'id' => \CI\Demographic::DEMOGRAPHIC_SEX_MALE,
        'name' => 'Male'
    );

    protected $aDemographicBoth = array(
        'id' => \CI\Demographic::DEMOGRAPHIC_SEX_BOTH,
        'name' => 'Either'
    );

    public function init()
    {
        $this->aDemographic = array(
            $this->aDemographicFemale['id'] => $this->aDemographicFemale,
            $this->aDemographicMale['id']   => $this->aDemographicMale,
            $this->aDemographicBoth['id']   => $this->aDemographicBoth
        );
    }
}