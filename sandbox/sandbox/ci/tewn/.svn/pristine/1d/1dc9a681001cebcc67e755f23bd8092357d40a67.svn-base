<?
	function tab_selected($status_a, $status_b) {
		// Hack for casting issue
		if ($status_a === "all" &&
		    $status_b === 0) {
			return "tab_unselected";
		}

		if ($status_a == $status_b) {
			$selected_class = "tab_selected";
		} else {
			$selected_class = "tab_unselected";
		}
		return $selected_class;
	}

?>

		<table cellpadding=0 border=0>
		<tr>
		<td>
			<? $selected_class = tab_selected($tab, ANALYTIC_LANDING_PAGE_LAYOUT); ?>
			<div class="tab <?=$selected_class?>" id="">
				<a class="<?=$selected_class?>" href="/management/landing_page">Landing Page Layout</a>
			</div>
                </td>
		<td>
			<? $selected_class = tab_selected($tab, ANALYTIC_LANDING_PAGE_ENGINE); ?>
			<div class="tab <?=$selected_class?>" id="">
				<a class="<?=$selected_class?>" href="/management/landing_page_engine">Landing Page Algorithms</a>
			</div>
                </td>
		</tr>
