/* CSS Document */
body,td,th {
	font-family: Arial, Helvetica, sans-serif;
	font-size: 12px;
}
#bd { overflow:visible; }
.left { width: 700px; float: left; }

.color1 /* see more style arrows */
{ background-color: #A42342; width: 15px; width: 15px; }
.color2 /* lighter bullet arrows */
{ background-color: #D66980;}

a:link, a:visited, a:active, .see-more a:link { color:#7B051B; }
a:hover { color:#C13D55; }

.content-block-right {
	float:right;
	width:300px;
}

.clear-both {
	clear:both;
}
.see-more
{
	clear: both;
	font-size: 10px;
	color: #555;
	font-family: Verdana, Arial, Helvetica, sans-serif;
	font-weight: normal;
	position:relative;
	top: 15px;
	text-transform:uppercase;
}
.TabbedPanelsTab a, .TabbedPanelsContent a { color: #ffffff; font-family:Verdana, Arial, Helvetica, sans-serif; font-weight:bold; }

.content-block-left .col1 {
	float: left;
	width: 435px;
	margin-top: 10px;
}
.content-block-left .col2 {
	float: right;
	width: 254px;
	margin-top: 10px;
}
.color_number { background-color: #000000; }

/*override ui.css*/
.whatshot-simple .yui-carousel .yui-carousel-item-selected { background-color:#C13D55; }
.whatshot .bd .whatshot-list a:hover { color:#C13D55; }
.whatshot .yui-carousel
{
	background-color:   #D6697C; }
.video_tabs .yesterday {background-color:#D6697C;}
.video_tabs .today {background-color:#C13D55;}
.video_title {color:#C13D55;}
/*2_column_8_links*/
.block1_2_columns_8_links {

	height: 245px;
}
.block1_2_columns_8_links .title {
	position: relative;
	top: 10px;
}
.block1_2_columns_8_links .links {
	position: relative;
	top: 10px;
	height: 155px;
	color: #5D0131;
	font-size: 14px;

	left: 0px;

}
.block1_2_columns_8_links .links .left {
	float: left;
	width: 160px;
	clear: left;
	line-height: 27px;
	margin-right:10px;
}
.block1_2_columns_8_links .links .right {
	clear: right;
	float: left;
	width: 155px;
	line-height: 27px;
}
.block1_2_columns_8_links .links .see-more { position:relative; top:15px; left:0px;}



