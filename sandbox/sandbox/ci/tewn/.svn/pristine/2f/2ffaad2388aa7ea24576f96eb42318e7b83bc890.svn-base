* {
	padding:0; margin:0;
}

body {
	margin: 0;
	padding: 0;
	background-image:url("http://cdn.crowdignite.com/img/crowdignite/bg_gradient.png");
	background-repeat:repeat-x;
	background-position:left top;
	background-attachment:scroll;
	font-family: Helvetica, Arial, sans-serif;
}

h1, h2, h3, h4, h5, h5, p {
	margin: 0;
	padding: 0;
	font-family: Helvetica, Arial, sans-serif;
}

a {
   color: #ffffff; cursor: pointer
}

a:hover {
	text-decoration: none;
}

img {
	border: none;
}

ul {
    display:block;
    list-style-position:outside;
    margin:0;
    padding:0;
}

.list-with-style-disc > li
{
  list-style-type: disc;
}

.list-with-style-decimal > li
{
  list-style-type: decimal;
}

.list-with-style
{
  padding: 0 0 0 15px;
}

li {
	list-style-type: none;
}

.center-text {
    text-align: center;
}

#header {
	width: 1080px;
	margin-left: auto;
	margin-right: auto;
	overflow: hidden;
	height: 140px;
}

#top {
	height:auto;
	position:relative;
	/* background: #364a6f;*/
	-moz-border-radius-bottomleft: 28px;
	-moz-border-radius-bottomright: 28px;
	-webkit-border-bottom-left-radius: 28px;
	-webkit-border-bottom-right-radius: 28px;
	border-bottom-left-radius: 28px;
	border-bottom-right-radius: 28px;
}

#header .logo {
	margin: 0 5px;
	overflow: hidden;
	float: left;
	display: inline;
}

#logo {
	margin-top:0px;
	margin-left:30px;
}

#tagline {
	text-shadow: 3px 1px 5px #111;
	color:#1e4164;
	text-align:center;
	font-size:16px;
}
#banner_ad {
	position:relative;
	top:20px;
	left:26px;
}
#other_stuff
{
	height:100px;
	text-align:right;
}

#webmaster_area
{
	float:left;
	position:relative;
	top:17px;
	left:520px;
	height: 20px;
	width:230px;
	padding:8px;
  /*text-shadow: 3px 3px 7px #111;	*/
	background-color:#DF6330;
  /*background-color: #1E4164;*/
	color:#ffffff;
	text-align:center;
	font-size:16px;
	-moz-border-radius: 12px;
	-webkit-border-radius: 12px;
	border-radius: 12px;
	behavior: url(/css/PIE.htc);

}

#widget_button
{
	position:relative;
	top:17px;
	left:6px;
	width:230px;
	padding:8px;
	text-shadow: 3px 3px 7px #111;
	color:#ffffff;
	text-align:center;
	font-size:16px;
	background: #d70d29;
	-moz-border-radius: 12px;
	-webkit-border-radius: 12px;
	border-radius: 12px;
}
#top_links
{
	color:#1e4164;
	margin-left:25px;
	margin-top:25px;
}
#top_links a
{
	color:#1e4164;
}

#links
{
	margin-left:30px;
	height:30px;
	font-size:18px;
	color: #ffffff;
	text-shadow: 3px 3px 7px #111;
}
table
{
   font-size:18px;
}
input
{
    height: 24px;
    font-size:18px;
}

select
{
   height: 24px;
   font-size:18px;
}
.extra_space
{
   margin-top:118px;
}

/* SIDE NAVIGATION */

.rightnav {
   background: #ffffff;
   padding:0px;
  -moz-border-radius: 28px;
  -webkit-border-radius: 28px;
  border-radius: 28px;
  text-align:center;
}


/* MAIN NAVIGATION */

.main {
}

#content h3 {
	padding-left: 16pt;
	padding-right: 6pt;
	padding-top: 16px;
	padding-bottom: 4px;
 	background-color: #df6330;
	height: 38px;
 	color: #ffffff;
 	text-shadow: 3px 3px 7px #111;
}

#landing_page_content h3 {
	background-color: #df6330;
	color: #ffffff;
	text-align: center;
}

.topContent
{
	-moz-border-radius:28px 28px 0 0 ;
	-webkit-border-radius:28px 28px 0 0 ;
	border-radius:28px 28px 0 0 ;
	height: 5px;
	behavior: url(/css/PIE.htc);
}

.bottomContent
{
	-moz-border-radius: 0 0 28px 28px;
	-webkit-border-radius: 0 0 28px 28px;
	border-radius: 0 0 28px 28px;
	height: 30px;
	behavior: url(/css/PIE.htc);
}

#plainarea {
	background-color: #ffffff;
	padding: 8px;
	overflow: hidden;
	zoom: 1;
	border-top: 1pt solid #cccccc;
}

#plainarea p {
	margin-top:18px;
}

#item {
	background-color: #ffffff;
	padding-top: 8px;
	padding-bottom: 8px;
	height: 80px;
	overflow: hidden;
	zoom: 1;
	border-top: 1pt solid #cccccc;
	border-left: 1pt solid #cccccc;
	border-right: 1pt solid #cccccc;
}
#item.selected {
    background-color: #e7a486;
    height:120px;
}

#item.alt {
	background-color: #f0f0f0;
}

#item:hover {
	background-color: #E7A486;
}

.itemtext {
	height:auto;
	display: table;
	overflow: visible;
}

.itemtext a {
	display: table-cell;
	vertical-align: middle;
	color: #000000;
	padding-right: 4px;
	font-size: 18px;
	text-decoration:none;
	font-weight:bold;
	height: inherit;
}
.itemtext a.bigger {
    font-size: 22px;
}
.itemtext a:hover {
	display: table-cell;
	vertical-align: middle;
	color: #000000;
	text-decoration:underline;
	font-weight:bold;
}

.itemtext_list {
/*	line-height: 80px;*/
	height: 80px;
}

.rightnav .itemtext a {
	display:block;
	text-align:center
}

.image {
	width: 140px;
	height: 140px;
	-moz-border-radius: 6px;
	-webkit-border-radius: 6px;
	margin-left: 12px;
	behavior: url(/css/ie-css3.htc);
	z-index:9;
}

.smallimage {
	width: 140px;
	height: 80px;
	-moz-border-radius: 6px;
	-webkit-border-radius: 6px;
	border-radius: 6px;
	margin-left: 12px;
	behavior: url(/css/ie-css3.htc);
	z-index:9;
}
.mediumimage {
	width: 160px;
	height: 120px;
	-moz-border-radius: 6px;
	-webkit-border-radius: 6px;
	border-radius: 6px;
	margin-left: 12px;
	behavior: url(/css/ie-css3.htc);
	z-index:9;
}

.largeimage {
	width: 300px;
	height: 170px;
	-moz-border-radius: 6px;
	-webkit-border-radius: 6px;
	border-radius: 6px;
	behavior: url(/css/ie-css3.htc);
	z-index:9;
}

#topnav {
	/* background: #ffffff;	*/
        /*padding:12px;*/
	-moz-border-radius: 28px;
	-webkit-border-radius: 28px;
	border-radius: 28px;
	text-align:center;
	margin-top:5px;
	height:89px;
}

#tabs
{
	margin-left: 40px;
}

#tabs table
{
	margin-bottom: -2px;
}

.tab
{
	padding-left:24px;
	padding-right:24px;
	padding-top:7px;
	padding-bottom:6px;
	margin-left:4px;
	font-family: Verdana;
	font-weight: bold;
	font-size: 14px;
	cursor:default;
	-moz-border-radius: 16px 16px 0 0;
	-webkit-border-radius: 16px 16px 0 0;
	border-radius: 16px 16px 0 0;
	behavior: url(/css/PIE.htc);
}
.tab_small
{
	padding-left:5px;
	padding-right:5px;
	padding-top:5px;
	padding-bottom:5px;
	margin-left:4px;
	font-family: Verdana;
	font-weight: bold;
	font-size: 12px;
	cursor:default;
	-moz-border-radius-topleft: 8px;
	-moz-border-radius-topright: 8px;
	-webkit-top-border-left-radius: 8px;
	-webkit-top-border-right-radius: 8px;
	border-top-left-radius: 8px;
	border-top-right-radius: 8px;
}

.tab_selected
{
	background:#DF6330;
	color:#ffffff;
	text-decoration: none;
}

.tab_unselected
{
	color:#1e4164;
	background:#e6e6e6;
	text-decoration: none;
}

#content_area {
	background-color: #f7f7f7;
	overflow: hidden;
	zoom: 1;
}

#content_area p {
	margin-top:5px;
}

.paginate
{
	color: #1e4164;
	text-decoration: underline;
	font-weight: bold;
	font-size: 20px;
}

/* Dashboard index */
.dashboard .column { padding-bottom:20px;}
.dashboard .box {
    background:#e7e7e7;
    color:#333;
    font-size:14px;
    font-weight: normal;
    margin:30px 0 10px 40px;
    -moz-border-radius: 5px;
    -webkit-border-radius: 5px;
    border-radius: 5px;
    border:1px solid #ccc;
    overflow:hidden;
    padding:0;
    width:90%;
}
.dashboard .box.sitelist { background:#e7e7e7 url(/img/crowdignite/dashboard_home_world.gif) bottom right no-repeat; }
.dashboard .box.quickadd { background:#e7e7e7 url(/img/crowdignite/dashboard_home_quick.gif) bottom right no-repeat; }
.dashboard .box.stats { background:#e7e7e7 url(/img/crowdignite/dashboard_home_pie.gif) bottom right no-repeat; }
.dashboard .box.chart { background:#e7e7e7 url(/img/crowdignite/dashboard_home_cloud.gif) bottom right no-repeat; }
.dashboard .box.quickadd p { padding: 0 30px 0px; }
.dashboard .box.quickadd p>a,.dashboard .box.quickadd p>a:hover  { color:#DF6330!important; text-decoration: underline; }
.dashboard .box.quickadd p.quickadd_button { padding:20px 0 20px 65px; }
.dashboard .box.right {
    width:540px;
    margin-left:40px;
}
.dashboard .top_area {
    background:#e1e1e1;
    font-size:18px;
    font-weight:bold;
    padding:7px 50px 7px 35px;
}
.dashboard .box.sitelist .top_area { background:#e1e1e1 url(/img/crowdignite/icon_world.png) 12px 10px no-repeat; }
.dashboard .box.news .top_area { background:#e1e1e1 url(/img/crowdignite/icon_newspaper.png) 12px 10px no-repeat; }
.dashboard .box.quickadd .top_area, .dashboard .box.stats .top_area{ background:#e1e1e1 url(/img/crowdignite/ci_fire_icon.png) 11px 5px no-repeat; }
.dashboard .box.chart .top_area{ background:#e1e1e1 url(/img/crowdignite/icon_chart_curve.png) 12px 10px no-repeat; }

.dashboard .top_area span.small {
    display:inline-block;
    height:25px;
    float:right;
    font-size:11px;
    font-weight: bold;
    line-height: 1.2em;
    padding:0 5px;
    width:60px;
}
.dashboard .area {
    padding:10px;
}
.dashboard .news .area { padding:0; }
.dashboard ul.websites { padding:0 10px 8px; }
.dashboard .box.news ul { padding-bottom:8px; }
.dashboard .box.news ul li {
    border-bottom:1px solid #ccc;
    font-size:12px;
    list-style: none;
    line-height: 1.5em;
    padding:5px 10px 10px;
}
.dashboard .box.news ul li span { color:#DF6330; font-weight: bold; }
.dashboard ul.websites li {
    border-bottom: 1px solid #ccc;
    color:#DF6330;
    font-size: 18px;
    padding:7px 10px;
    list-style:disc inside!important;
    line-height:16px;
}
.dashboard ul.websites li:last-child,
.dashboard .box.news ul li:last-child{
    border:none;
    padding-bottom:0;
}
.dashboard ul.websites li span {
    color:#333;
    display:inline-block;
    font-size:14px;
    text-align: center;
    vertical-align: top;
    width:15%;
}
.dashboard ul.websites li .sitename { text-align:left; width:60%; }
.dashboard ul.websites li.head { font-weight:bold; list-style:none!important; padding:0 0 7px 30px; }
.dashboard ul.websites li.head span{ font-size:12px; text-align:center; }

.green,
.dashboard ul.websites .active,
.profile ul.websites .active,
.element_container .element .info b.green { color:#060!important; }

.red,
.dashboard ul.websites .inactive,
.profile ul.websites .inactive,
.element_container .element .info b.red { color:#800!important; }

.yellow {
    color:#e7ae3b;
}

.chart_small {
	display:inline-block;
	cursor: default;
	height:200px;
	margin:5px 5px 0 0;
	width:300px;
}
.inside_block_ui {
	background:#fff;
	border:3px solid #333;
	position: relative;
	/* left:-50%;
	top:-50%; */
}

/* eo Dashboard index */

/* BD chart */
.dashboard .box.chart .controls { margin:10px 0 0 20px; }
.dashboard .box.chart .controls div.select { margin:8px 5px; }
.dashboard .box.chart .controls select { font-size:14px; }
.dashboard .box.chart .controls h4 { font-size:18px; }
.dashboard .box.chart .controls label { display:inline-block; width:118px; }
.dashboard .box.stats table {
    padding-bottom:10px;
    width:100%;
}
.dashboard .box.stats table th { border-bottom:1px solid #ddd; }
.dashboard .box.stats td { text-align: center; }
.dashboard  #bd_graphs {
    margin:30px 0 0 40px;
}
table#deals {
    margin: 0 10px 20px;
    width: 98%;
}

/* */

/* Dashboard websites */
#selectWebsite {
    font-size:16px;
    font-weight:bold;
    height:auto;
}
a#add_website_button, a#add_widget_button {
    background:url(/img/crowdignite/btn_addwebsite.jpg) 0 0 no-repeat;
    display:inline-block;
    height:48px;
    width:291px;
}
a#add_website_button:hover, a#add_widget_button:hover {
    background-position: 0 -48px;
}
a#add_website_button span, a#add_widget_button span{
    color:#fff;
    display:inline-block;
    font-size:20px;
    font-weight:bold;
    margin:10px 0 0 50px;
}
.element_container {
    background:#f7f7f7;
    border-bottom:1px solid #949494;
    margin-top:20px;
}
.element_container.inactive { display:none; }
.element_container.active { display:block; }
.element_container .element {
    display:block;
    background:none;
    border-top:1px solid #949494;
    padding:20px 0 20px 20px;
    width:420px;
}
.element_container .element:hover,
.element_container .selected { background: #e4e4e4; }
.element_container .element .title { cursor: pointer; font-size:18px; font-weight: bold;}
.element_container .element .title span.websitename { background: url(/img/crowdignite/website_arrows.png) 0 2px no-repeat; float:left; padding-left:20px; }
.element_container .selected .title span.websitename { background-position: 0 -15px;}
.element_container .element .title span.websitename a,
.element_container .element .title span.websitename a:visited { color:#333; text-decoration: none; outline:none; }
.element_container .element .title span.websitename a:hover { color:#555!important; }
.element_container .element .title span.rate { float:right; padding-right:10px; }
.element_container .element .info { display:none; }
.element_container .selected .info { display:inline-block!important; }
.element_container .element .info p {
    color:#777;
    display:block;
    font-size:14px;
}
.element_container .element .buttons, .profile .buttons { margin:10px 0 0; }
.profile .buttons a.button,
.element_container .element .buttons a.button,
.element_container .element .buttons a.button:visited {
    background:#df6330;
    color:#fff!important;
    display:inline-block;
    font-size:12px;
    font-weight:bold;
    height:20px;
    padding:4px 10px 0;
    text-decoration: none;
    -webkit-border-radius: 3px;
    -khtml-border-radius: 3px;
    -moz-border-radius: 3px;
    border-radius: 3px;
    margin:0 2px 0 0;
}
.profile .buttons a.button span,
.element_container .element .buttons a.button span { float:left; }
.profile .buttons a.button span.icon,
.element_container .element .buttons a.button span.icon {
    display:inline-block;
    height:15px;
    width:15px;
    margin:1px 5px 0 0;
}
.element_container .element .buttons a.chart span.icon { background:url(/img/crowdignite/dashboard_websites_icons.png) 0 1px no-repeat; height:12px; }
.profile .buttons a.rename span.icon,
.element_container .element .buttons a.rename span.icon,
.element_container .element .buttons a.edit span.icon { background:url(/img/crowdignite/dashboard_websites_icons.png) 0 -12px no-repeat; height:12px; }
.element_container .element .buttons a.delete span.icon { background:url(/img/crowdignite/dashboard_websites_icons.png) 0 -28px no-repeat; height:14px; }
.element_container .element .buttons a.widgets span.icon { background:url(/img/crowdignite/dashboard_websites_icons.png) 0 -40px no-repeat; height:12px; }
.element_container .element .buttons a.code span.icon { background:url(/img/crowdignite/dashboard_websites_icons.png) 0 -52px no-repeat; height:12px; }
.element_container .element .buttons a.button:hover,
.profile .buttons a.button:hover
{ background: #ea7241; color:#fff; }
.element_container .element .buttons a.active { background:#b43501!important; color:#fff; }
.element_container .element .info b { color:#333; }
.profile b span, .element_container .element .info b span { font-size:25px; display:inline; line-height: 14px; vertical-align: top; }
.profile .username, .profile .yourwebsites {
    background:url(/img/crowdignite/ci_fire_icon.png) no-repeat left 16px;
    font-size:28px;
    font-weight:bold;
    display:inline-block;
    height: 30px;
    padding:10px 0 0 25px;
}
.profile .yourwebsites { background:none; font-size:20px; padding:18px 0 0 0px; height:22px}
.profile ul.websites {
    padding-left:30px;
    padding-bottom:8px;
}
.profile ul.websites li { color:#DF6330; font-size: 18px; padding-bottom:10px;  list-style:disc!important; line-height:16px; }
.profile ul.websites li span { color:#333; font-size:14px; font-weight: bold;}
.profile ul.websites li a { font-size:14px; }
.profile a, .profile a:hover { color:#DF6330!important; text-decoration: none; }
.profile .buttons { padding:10px 0 0 0; }
.profile .bold {font-weight: bold;}
.profile .properties p { border-top:1px solid #ccc; padding:10px 0 5px 20px; width:90%; }
.profile .properties_right p { border-top:1px solid #ccc; padding:10px 0 5px 20px; width:88%; }
/* eo Dashboard websites */

/* Website moderate */
#regex_tools {
    display:block;
    height:15px;
    margin:10px 0 0 10px;
}
#regex_tools li {
    float:left;
    list-style: none;
    display:inline-block;
}
#regex_tools li a {
    display:inline-block;
    padding:4px 10px 4px 20px;
    margin-right:15px;
}
#regex_tools li a#rtool {background:url("/img/crowdignite/icon_tool_page.png") no-repeat 0 6px; }
#regex_tools li a#btn_regular_expressions {background:url("/img/crowdignite/icon_tool.png") no-repeat 0 6px; }
#regex_tools li a#add {background:url("/img/crowdignite/icon_page_add.png") no-repeat 0 6px; }
#regex_tools li a#pages {background:url("/img/crowdignite/icon_page_copy.png") no-repeat 0 6px; }
#regex_tools li a#edit {background:url("/img/crowdignite/icon_page_edit.png") no-repeat 0 6px; }

#regular_expressions {
    background:#eee;
    margin:0 0 10px 0;
}
#regular_expressions ul { padding:10px 0 10px 30px; }
#regular_expressions ul li { list-style: disc; color:#DF6330; }
#regular_expressions ul li span { color:#333; }
.website_moderate {
    background:#e7e7e7;
    font-size:13px;
    margin:0 20px 10px 10px;
    overflow: hidden;
    width:1010px;
    -moz-border-radius: 10px;
    -webkit-border-radius: 10px;
    border-radius: 10px;
    -moz-box-shadow: 0 0 2px rgba(0, 0, 0, 0.2), 0 1px 1px rgba(0, 0, 0, 0.3); /* Firefox */
    -webkit-box-shadow: 0 0 2px rgba(0, 0, 0, 0.2), 0 1px 1px rgba(0, 0, 0, 0.3); /* Safari, Chrome */
    box-shadow: 0 0 2px rgba(0, 0, 0, 0.2), 0 1px 1px rgba(0, 0, 0, 0.3); /* CSS3 */
    border-top:1px solid #fff;
}
.website_moderate a {
    color:#cb3f05!important;
    text-decoration:none;
}
.website_moderate a:hover { text-decoration:underline;}
.website_moderate b { font-weight: bold; }
 #top_area {
    font-weight: bold;
    cursor:pointer;
    padding:7px 10px;
}
.website_moderate.selected #top_area { background:#C4C4C4; }
.website_moderate.selected {
    background: #e7e7e7;
    /* filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#e7e7e7;', endColorstr='#cccccc');*/ /* for IE */
    background: -webkit-gradient(linear, left top, left bottom, from(#e7e7e7), to(#cccccc)); /* for webkit browsers */
    background: -moz-linear-gradient(top,  #e7e7e7,  #cccccc); /* for firefox 3.6+ */
}

.website_moderate #top_area span.btn_toggle {
    display:inline-block;
    background: url("/img/crowdignite/website_arrows.png") no-repeat scroll 0 -2px transparent;
    padding-left: 20px;
    text-indent:-9999px;
    width:1px;
}
.website_moderate.selected #top_area span.btn_toggle{
    background-position: 0 -18px;
    cursor:pointer;
}

.website_moderate #top_area span,
.website_moderate #top_area span .error_message {
    display:inline-block;
    width:300px;
}
.website_moderate #top_area span.url_input {
    background:url("/img/crowdignite/icon_link.png") no-repeat 0 6px;
    padding-left:22px;
    width:250px;
}
.website_moderate table.contain span.alexa {
	background:url("/img/crowdignite/icon_alexa.png") no-repeat right 5px;
	display:inline-block;
	color:#333;
	font-weight:bold;
	padding:4px 22px 2px 0;
}
.website_moderate table.contain div.website_status {
	display:inline-block;
}
.website_moderate table.contain div.website_user_field {
	display:inline-block;
	margin-top: 5px;
}
.website_moderate table.contain div.website_user_field label {
	width: 70px;
	display: inline-block;
}
.website_moderate #top_area span.url {
    background:url("/img/crowdignite/icon_world.png") no-repeat 0 5px;
