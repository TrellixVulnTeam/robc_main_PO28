class Logger_Analytic_Element_Basic extends Logger_Analytic_Element_Abstract
{
    /**
     * Register the value
     *
     * @param mixed $value The value to be registered
     *
     * @return void
     */
    public function registerValue($value)
    {
        $value = is_null($value) ? '' : $value;
        $this->value = $value;
    }
}