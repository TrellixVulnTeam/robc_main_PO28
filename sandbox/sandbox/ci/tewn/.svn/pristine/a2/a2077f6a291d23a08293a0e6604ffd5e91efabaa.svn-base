/* ========== Importing fonts ========== */
@font-face {
	font-family:'BorregoRegular';
	src:url(/img/liveoutdoors/fonts/borrego-webfont.eot);
	src:local(' '),
        url(/img/liveoutdoors/fonts/borrego-webfont.woff) format('woff'),
        url(/img/liveoutdoors/fonts/borrego-webfont.ttf) format('truetype'),
        url(/img/liveoutdoors/fonts/borrego-webfont.svg#webfontIUHMg6yJ) format('svg');
	font-weight: normal;
	font-style: normal;
}
/* ========== Font Families ========== */
body {
	font-family: Arial, San-serif;
	text-transform: none;
}
h1, #nav_top a {
	font-family: 'BorregoRegular', Impact, Arial, sans;
	font-weight: normal;
	text-transform: uppercase;
}
.wf-loading h1,
.wf-loading #janews_section h1,
.wf-loading #janews_home h2,
.wf-loading div.com_search h2,
.wf-loading .module .poll_question,
.wf-loading .module h3,
.wf-loading .com_poll h3,
.wf-loading .type_breadcrumbs,
.wf-loading div.fpss-introtext .slidetext h1 a, /* eff FPSS */
.wf-loading div.fpss-introtext .slidetext h1 a:hover,
.wf-loading #nav_top a {
	visibility: hidden;
}

#content .article .meta,
#janews_section .article-tools,
#nav_top .module.type_php #header-showdate,
#footer #footer-showdate,
#footer .copyright,
a.readon {
	font-family: Arial, Helvetica, sans;
}


/* ========== Common Colours ========== */

a,
#navi-outer #fpss-container_next,
#navi-outer #fpss-container_prev,
div#navi-outer li.noimages a,
div#navi-outer li.noimages a:hover {
	color: #000
}

div#navi-outer li a:hover,
div#navi-outer li a.navi-active {
	border: 1px solid #333;
}

/* White */
#roadblock,#page_body, #footer {
	background: white;
}

div.fpss-introtext .slidetext h1 a,
div.fpss-introtext .slidetext h1 a:hover {
	color: white;
}

/* Dark Grey */
#footer .sitelinks, #footer .sitelinks a {
	color: #808080;
}

/* Black */
#footer, #footer a {
	color: #000;
}
h3 {
	font-size: 1.65em;
	margin: 0 0 -.5em;
}
h4 {
	font-size: 1.2em;
	margin: .75em 0 -.5em;
}

/* ========== Header/Footer Metadata ========= */
#nav_top #header-showdate,
#footer #footer-showdate,
#footer .copyright {
	font-size: 10px;
	line-height: 12px;
}
/* ========== Global positioning rules ========== */
html {
	font-size: 16px;
	min-width: 988px;
}

body {margin:0;}

#wrapper {
	width:973px;
	margin:0 auto;
}

#header {position: relative; height: 176px; }

#header a.logo {
	display: block;
	height: 128px;
	width: 620px;
	margin: 0;

	text-indent: -5000em;
}

#page_body,
#roadblock {
	text-align:left;
	padding: 5px 4px;
}

#column_main {
	width: 473px;
	margin: 0 5px;
}

/* Content padding */
#column_main > div {
	padding: 0 5px;
}

/* ========== Global positioning rules ========== */
body {
	background: #faf8fb url(/img/liveoutdoors/bg.jpg) no-repeat center top;
	font-size: .9em;
}

a {
	border: none;
	text-decoration: none;
	outline: none;
}

a:hover {
	text-decoration: underline
}

a img {border: none;}

#roadblock,
#page_body, #footer {
	border-left: 1px solid #808080;
	border-right: 1px solid #808080;
}

/* ========== Top Nav ========== */
#nav_top .menu,
#nav_top li {
	margin: 0;
	padding: 0;
	list-style: none;
}

#nav_top a {
	font-size: 2.25em;
	letter-spacing: 1px;
	color: white;
	text-decoration: none;
}

#nav_top ul {
	background: white url(/img/liveoutdoors/nav.png) no-repeat right 0;
	overflow: hidden;
}

#nav_top li {
	display: block;
	float: left;
	border-right: 5px solid white;
}

#nav_top .menu a {
	display: inline-block;
	background: white url(/img/liveoutdoors/nav.png) no-repeat 0 -96px;
	height: 48px;
}

#nav_top .menu span {
	background: transparent url(/img/liveoutdoors/nav.png) no-repeat right -96px;
	display: inline-block;
	margin: 0 0 0 5px;
	padding: 16px 16px 0 4px;
	line-height: 32px;
}

#nav_top .menu a:hover {
	background-position: 0 -144px;
}

#nav_top .menu span:hover {
	background-position: right -144px;
}

/* 1002x1002 */
#nav_top .ad {
	width: 160px;
	height: 42px;

	position: absolute;
	bottom: 3px;
	right: 207px;
}

/* Search module */
#nav_top .type_search {
	width: 196px;
	height: 40px;
	border: 1px solid #e1e1e1;

	position: absolute;
	bottom: 3px;
	right: 3px;
}

#nav_top .type_search .inputbox {
	display: block;
	padding: 5px 40px 5px 5px;
	border: 1px solid #ebebeb;
	width: 149px;
	height: 28px;
	line-height: 30px;

	position: absolute;
	top: 0;
	left: 0;

	font-size: 17px;
	color: #666;
}

#nav_top .type_search .button {
	font-size: 0;
	margin: 0;
	padding: 0;
	border: none;

	width: 40px;
	height: 38px;

	position: absolute;
	top: 1px;
	right: 1px;

	color: white;
	cursor: pointer;
	background: transparent url(/img/liveoutdoors/searchbutton.png) no-repeat center center;
}

/* Header Date */
#nav_top .module.type_php #header-showdate {
	position: absolute;
	right: 3px;
	top: 106px;
}

/* ========== Search ========== */
div.com_search #searchForm {
	padding: 0 0 10px 0;
}

div.com_search .search_results .result {
	padding: 10px 5px;
}

div.com_search .search_results .result .title {
	font-size: 16px;
	line-height: 1.1;
	padding: 0;
}

/* ========== Footer ========== */
#footer {
	background: white url(/img/liveoutdoors/bg_footer.png) no-repeat left bottom;
	min-height: 38px;
	padding: 2.5em 60px 28px 258px;
	margin: 0;
	position: relative;
	font-size: .9em;
}

#footer .links {
	text-transform: uppercase;
	font-weight: bold;
}

#footer #footer-showdate,
#footer .copyright {
	position: absolute;
	bottom: 14px;
	left: 260px;
}
#page_body #footer { border:none; }
#footer #footer-showdate {left: 23px;}

#content { margin-left:0; }
#content a, #content a:hover { text-decoration:none; }
.clearfloat { clear:both; height:0; margin:0; padding:0; }
.container_12, .container_16, div#page_body, div[class*="container_"]  { width:auto; }
div.main>div#page_body { width:auto; border:0; padding:0; }
.grid_12 .grid_3 .text  { text-align:center; }
.main .text p a { font-weight: bold; font-size: 1.2em; line-height: 1.1em;}

/* signup */
div.row { width:970px; margin:0 auto; }
div.row #content { background: #fff; margin:0 auto; padding: 10px 20px; width: 925px; }

div.main #content form div.submit input{
    background: url(/img/liveoutdoors/btnsignup.png) no-repeat;
    border:none;
    height: 69px;
    margin-top:10px;
    text-indent: -9999px;
    width:282px;
}
div.main #content form div.submit input:hover {
    background-position:0 -68px;
}
.grid_12 .grid_3 { margin-right:5px; }
h3.topContent { display: none; }
div.row #content ul, div.main  #content li { list-style: none;  }
div.row #content .column { width:910px; }
div.row #content .column  div#footer { border:0; background:none!important; }
div.submit { padding-left:125px; text-align:left; }
div.submit input:hover {
    background-position:0 -68px;
}
#create_form {  position:relative;}
#create_form ul li.last {
    border-bottom:1px solid #ccc;
    height: 7em!important;
    padding-bottom:2em!important;
}
#create_form ul { padding-top:20px; }
#create_form .title {
    width:95%;
    margin:10px auto 10px;
    padding:10px 0;
    border-bottom:1px solid #ccc;
}
#create_form .title h2 {
    background:url(/img/crowdignite/ci_fire_icon.png) no-repeat 0 5px;
    color:#333;
    display:block;
    margin:0;
    padding:0 0 0 25px;
    font-size:27px;
    letter-spacing: -0.05em;
}
#create_form .title p {
    color:#666;
    font-size:14px;
    line-height: 1em;
    margin:2px 0 10px 0;
    padding-left:25px;
    text-align:left;
}
#create_form div.control {
    position:relative;
    display:block;
    float:left;
    line-height:2em;
    margin:0;
    padding:0;
}
#create_form label.error {
    background:#FFF2BF url(/img/crowdignite/icon_deny.png) no-repeat 5px 5px;
    border:1px solid #e5d595;
    color:#900;
    display:block;
    font-size:14px;
    line-height:1.1em;
    margin:0;
    padding:6px 0 6px 29px;
    position: absolute;
    text-align:left;
    top: 0;
    left:280px;
    width: 350px;
    z-index: 100;
}
#create_form ul {
    width: 100%; min-height:400px;
}
#create_form ul li {
    width:95%;
    margin:0 auto;
}
#create_form ul.form>li {
    height:3em;
    padding:1em 0 0.2em 0;
}
#create_form ul.form>li>label {
    color:#333;
    float:left;
    line-height:2em;
    margin:0;
    padding:0 30px 0 0;
    text-align:right;
    width:120px;
}
#create_form ul.form li span.terms {
    color:#333;
    font-size:14px;
    display: inline-block;
    padding-left:8px;
}
#create_form ul.form li label.terms a { color:#333!important; text-decoration: underline; }
#create_form ul li.alt {
    background:#fff;
}
#create_form ul li.focus_line {
    background:#fdf7df;
}
#create_form div.join_text {
    position:absolute;
    right:30px;
    text-align:left;
    top:120px;
    width:380px;
}
#create_form div.join_text #img_what_is_ci {
    background: url("/img/crowdignite/what_is_ci.png") no-repeat;
    display: inline-block;
    height: 205px;
    margin:10px -10px 0 0;
    width: 374px;
}
#create_form div.join_text h2 {
    color:#E26832;
    font-size: 25px;
    letter-spacing:-0.03em;
    padding:0;
}
#create_form div.join_text p {
    color:#333;
    font-size:14px;
    width:90%;
}
#create_form div.join_text p a {
    color:#E26832;
    text-decoration:none;
}
/* Eo signup */