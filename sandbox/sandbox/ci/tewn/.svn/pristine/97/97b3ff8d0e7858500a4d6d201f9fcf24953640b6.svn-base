<style>
body { 
    background:#f8f8f8;
    min-width: 340px!important;
}
ul.navigation {
    margin:110px auto 0 auto;
    padding:0 40px 0 0;
    position:relative;
}

.top_content_spacer { display:inline-block; margin:0;}

#content, .bottom_content_spacer { float:none; margin-left:0!important; margin-right:0!important; display:block; }
div#page_body { background:none; width:1290px; min-width:0!important; }
#interior_content, .container_12 .grid_12 { background:none; width:100%; float:none; border:0; padding:0; /*width: 1080px;*/}

#links_content {
    border-radius:0;
    float:none;
    clear:both;
    display:block;
    -moz-border-radius:0;
    -webkit-border-radius:0;
    margin:0 auto!important;
    padding:0;
    width:100%;
}
#paginate { clear:both; padding-top:10px; margin:0; }
#paginate a { color: #333; text-decoration:none;}
#paginate a:hover { text-decoration:underline;}
/* small_grid */
.grid_7,.grid_5 { float:none; margin:0; }

.container_12 .grid_5,
.container_12 .grid_7 > .grid_7,
.container_12 .grid_5 > .grid_5 {
    width: auto;
    margin:0;
}
#links_content {
    /* width:1003px; */
}
.grid_12 .grid_5 img {
    border-radius:0;
    -moz-border-radius:0;
    -webkit-border-radius:0;
}
.bottom_content_spacer { margin-bottom:15px; }
.grid_12>.grid_7 {
    height:auto;
    width:auto;
    display:inline;
}

/* font loader styles */
.wf-loading .item div.text p a {
    visibility:hidden;
}
.wf-loading .item div.text p.description a {
    font-weight:normal;
    letter-spacing:0.01em;
}
.wf-active .item div.text p a {
    font-family:"Droid Serif", "Times New Roman", Serif;
    font-size:18px!important;
    font-weight:normal;
    letter-spacing:-1px;
}
.wf-active .item { display:block!important; height:auto!important;}
.wf-active .item div.text p.description a {
    font-family:"Open Sans", Arial, sans-serif;
    font-size:12px!important;
    font-weight:normal;
    letter-spacing:0.01em;
}
/* all boxes */
.item {
        display:none;
        background-color: #FFFFFF;
        border:1px solid white;
        border-radius: 3px;
        -moz-border-radius: 3px;
        -webkit-border-radius: 3px;
        box-shadow: 0 0 2px rgba(0, 0, 0, 0.35);
        -moz-box-shadow: 0 0 2px rgba(0, 0, 0, 0.35);
        -webkit-box-shadow: 0 0 2px rgba(0, 0, 0, 0.35);
        float:left;
        height:290px;
        margin:3px 3px!important;
        overflow: hidden;
        padding: 6px;
        cursor: pointer;
        text-align:left;
        width: 300px!important;
        float:left;
}
.item:hover {
        background-color: #E6E6E6;
        /*border: 1px solid #B3B3B3;*/
}
.item .image {
        border-radius:0px!important;
        -moz-border-radius:0px!important;
        -webkit-border-radius:0px!important;
        display: block;
        height: 250px;
        overflow: hidden;
        width: 300px;
}
.item .image a {
        display:block;
}
.item .image img{
        border:0;
        height:250px; /* 145px; */
        width:auto;
}
.item div.text p{
        color: #209E9C;
        font-size: 20px;
        font-weight: 300;
        letter-spacing:-0.05em;
        line-height:0.1em;
        margin:5px 0 0 0;
}
.item div.text p a{
        border:0;
        color: #555!important;
        display: inline;
        font-size:22px;
        font-weight:normal;
        line-height:24.2px;
        letter-spacing: -1px;
        margin-top:0px;
        /* max-width: 220px; */
        overflow: hidden;
        text-decoration: none;
        /* width:100%; */
}
.item div.text p.description a{
        border:0;
        color: #777!important;
        display: inline;
        font-size:12px;
        font-weight:normal;
        line-height:1.3!important;
        letter-spacing:0.01em;
        margin-top:8px;
        /* max-width: 100%; */
        max-height:120px;
        padding:0 0 5px 0;
        overflow: hidden;
        text-decoration: none;
        /* width:100%; */
}
.item:hover div.text p a { color:#333!important; }
.item div.text p.description a:hover{ text-decoration:none; color:#777; }

/* large items */
.grid_12 .large_item {
        height: auto!important;
        width: 620px !important;
        display:block;
}
.grid_12 .large_item .image img {
        border:0;
        width:auto!important;
        height:250px!important;
}
.grid_12 .large_item .image,
.grid_12 .large_item .image a {
        overflow: hidden;
        width: 300px!important;
        height: 250px!important;
}
.grid_12 .large_item div.text {
    position: absolute;
    top:20px;
    right:15px;
    width:300px;
}

.grid_12 .large_item a.button {
    background: #DF6330;
    border-bottom:1px solid #ae4419;
    cursor: pointer;
    display: block;
    height:30px;
    line-height: 1em;
    margin:20px auto 0;    
    outline: none;
    text-align:center;
    white-space: nowrap;
    width:140px;
    -moz-border-radius: 3px;
    -webkit-border-radius: 3px;
    -khtml-border-radius: 3px;
    border-radius: 3px;
    -moz-box-shadow:    0 1px 2px rgba(0, 0, 0, 0.2);
    -webkit-box-shadow: 0 1px 2px rgba(0, 0, 0, 0.2);
     box-shadow:        0 1px 2px rgba(0, 0, 0, 0.2);
} 
.grid_12 .large_item a.button span {
    color:#fff;
    display: block;
    font-family:helvetica,Arial,sans-serif;
    font-size:14px;
    line-height:30px;
    text-shadow: 0 1px 1px #a5370a; 
}
.grid_12 .large_item a.button:hover {
    background:#d25724;
}
.bottom_content_spacer { margin-bottom:15px; }
.grid_12>.grid_7 {
    height:auto;
    width:auto;
    display:inline;
}
.grid_12 .item:last-child { clear:none; }
#header {
    background:#fff;
    border-bottom: 1px solid #bbb;
    box-shadow: 0 1px 5px #bbb; 
    -moz-box-shadow: 0 1px 5px #bbb;
    -webkit-box-shadow: 0 1px 5px #bbb;      
    height:50px;
    margin-bottom:12px;
    padding:4px 0;
    width:100%!important;
}
#header .logo {    
    height:50px;
    left:4%;
    position:absolute;    
    top:0;
    width:131px;    
}
#header .logo a {
    background:url(/img/crowdignite/ci_logo_50px.jpg) no-repeat;    
    display:block;
    height:50px;
    width:131px;
    margin:0;
    padding:0;
}
#header .logo a img{ display:none; }
#header a {
    color:#666;
    font-family:helvetica, arial, sans-serif;
    font-size:12px;
    text-decoration:none;
}
#header a:hover { text-decoration:underline; }
#header .header_wrapper { height:45px; margin:0 auto; position:relative; width:1290px;}
#tabs {
    float: right;
    height: 30px;
    border:none;
    margin: 11px 0 0;
    padding:0;
}
#tabs .tab {
    border-right:1px solid #ccc;
    -moz-border-radius:0; 
    -webkit-border-radius: 0;
    border-radius:  0;    
    margin: 0;
    padding:0 15px; 
}
#tabs table .tab,
#tabs table .tab_unselected,
#tabs table .tab_selected {
    background:none!important;
}
#tabs table .tab_selected a { color:#333;}
#tabs table .tab_selected a:hover { text-decoration:none;}
#tabs table td:last-child .tab { border-right:none;}
#topnav { 
    width:auto;
    height:auto;
}
#webmaster_area { 
    background:#fff;
    color:#ccc;
    float: right;
    height:30px;
    left:auto;
    padding:0;
    margin: 6px 0 0;
    top:auto;

}
#webmaster_area a{ color:#666; font-size:12px; }
.top_content_spacer {
    display:none;
}
#footer {
    background:#fff;
    border-top:none;
    box-shadow: 0 -1px 5px #bbb; 
    -moz-box-shadow: 0 -1px 5px #bbb;
    -webkit-box-shadow: 0 -1px 5px #bbb;       
    color:#666;
    padding-right:0!important;
    padding-left:0!important;
}
/* media requests */
@media (max-width: 1200px) {
    div#page_body { width:100%!important; }
    #header .header_wrapper { width:100%!important;}
}
@media (max-width: 650px) {
    .grid_12 .large_item {
        width: 300px !important;
    }
    .grid_12 large_item div.text p {
        font-size: 22px !important;
        line-height: 24px !important;
    }
    .grid_12 .large_item .image img {
        border:0;
        width:auto!important;
    }    
    .grid_12 .large_item div.text {
        position: relative;
        top:auto;
        left:auto;
        right:auto;
        width:300px;
    }
    .grid_12 .large_item a.button {
        display: none;
    }
    .wf-active #header .poweredby a,
    .wf-active #header .poweredby span {
        display:none!important;
    } 
}
</style>

<script src="/js/jquery.masonry.min.js"></script>
<script src="/js/jquery-ui-1.8.custom.min.js"></script>
