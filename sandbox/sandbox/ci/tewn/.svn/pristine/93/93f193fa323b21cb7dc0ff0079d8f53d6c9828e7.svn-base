

body {
	margin: 0;
	padding: 0;
	background: #c0c0bf;
	font-family: Helvetica, Arial, sans-serif;
}

h1, h2, h3, h4, h5, h5, p {
	margin: 0;
	padding: 0;
	font-family: Helvetica, Arial, sans-serif;
}

a {
   color: #ffffff;
}

a:hover {
	text-decoration: none;
	color: #5a8fdb;
}

img {
	border: none;
}

ul {
    display:block;
    list-style-position:outside;
    margin:0;
    padding:0;
}

li {
	list-style-type: none;
}

