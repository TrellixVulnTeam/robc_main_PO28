/* MAIN NAVIGATION */

.main {
}

#content h3 {
	padding-left: 16pt; 
	padding-right: 6pt;
	padding-top: 16px; 
	padding-bottom: 4px;
	background-color: #364a6f;
	height: 38px;
	color: #ffffff; 
	text-shadow: 3px 3px 7px #111;	
}


.topContent
{
  -moz-border-radius-topleft: 28px; 
  -moz-border-radius-topright: 28px; 
  -webkit-border-top-left-radius: 28px;
  -webkit-border-top-right-radius: 28px;
  border-top-left-radius: 28px;   
  border-top-right-radius: 28px;  
}

.bottomContent
{
  -moz-border-radius-bottomleft: 28px; 
  -moz-border-radius-bottomright: 28px;  
  -webkit-border-bottom-left-radius: 28px;
  -webkit-border-bottom-right-radius: 28px;
  border-bottom-left-radius: 28px;   
  border-bottom-right-radius: 28px;   
}

#plainarea {
	background-color: #ffffff;
	padding: 8px;
	overflow: hidden;
	zoom: 1;
	border-top: 1pt solid #cccccc;
}

#plainarea p {
	margin-top:18px;
}

#item {
	background-color: #ffffff;
	padding-top: 8px;
	padding-bottom: 8px;
	height: 80px;	
	overflow: hidden;
	zoom: 1;
	border-top: 1pt solid #cccccc;
}
#item.selected {
    background-color: #ffb11a;	  
    height:120px;
}

#item.alt {
	background-color: #f0f0f0;
}

#item:hover {
	background-color: #a8cbff; 
}

#itemtext {
	height:80px;
	display: table;
	overflow: visible;
}

#itemtext a {
	display: table-cell; 
	vertical-align: middle; 
	color: #000000;
	padding-right: 4px;
	font-size: 18px;
	text-decoration:none;
	font-weight:bold;
}
#itemtext a.bigger {
    font-size: 22px;	
}
#itemtext a:hover {
	display: table-cell; 
	vertical-align: middle;
	color: #000066;
	text-decoration:underline;
	font-weight:bold;
}

.itemtext_list {
	line-height: 80px;
	height:80px;
}

.image {
	width: 140px;
	height: 140px;
	-moz-border-radius: 6px;
	-webkit-border-radius: 6px;
	margin-left: 12px;
}

.smallimage {
	width: 140px;
	height: 80px;
	-moz-border-radius: 6px;
	-webkit-border-radius: 6px;
	border-radius: 6px;
	margin-left: 12px;
}
.mediumimage {
	width: 160px;
	height: 120px;
	-moz-border-radius: 6px;
	-webkit-border-radius: 6px;
	border-radius: 6px;
	margin-left: 12px;
}
.largeimage {
	width: 300px;
	height: 170px;
	-moz-border-radius: 6px;
	-webkit-border-radius: 6px;
	border-radius: 6px;
	margin-left: 12px;
}

