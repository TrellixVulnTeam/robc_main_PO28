/* SIDE NAVIGATION */

.rightnav {
   background: #ffffff;	
   padding:0px;
  -moz-border-radius: 28px; 
  -webkit-border-radius: 28px;
  border-radius: 28px;   
  text-align:center; 
}


/* MAIN NAVIGATION */

.main {
	
}

#bottomContent {
	padding-left: 16pt; 
	padding-right: 6pt;
	padding-top: 16px; 
	padding-bottom: 4px;
 	background-color: #df6330;
	height: 38px;
 	color: #ffffff; 
 	text-shadow: 3px 3px 7px #111;	
}

#landing_page_content h3 {
	background-color: #df6330;
	color: #ffffff; 
	text-align: center;                           
}

.top_content_spacer
{
	-moz-border-radius:28px 28px 0 0 ; 
	-webkit-border-radius:28px 28px 0 0 ;
	border-radius:28px 28px 0 0 ;   
	height: 5px;
	behavior: url(/css/PIE.htc);
	width: 100%;
	background-color: #df6330;
}

.bottom_content_spacer
{
	-moz-border-radius:0 0 28px 28px ; 
	-webkit-border-radius:0 0 28px 28px ;
	border-radius:0 0 28px 28px ;   
	min-height: 5px;
	behavior: url(/css/PIE.htc);
	width: 100%;
	background-color: #df6330;
	margin-left: 0px;
}

div#page_body, div[class*='container_'] {
	width: 1024px;
}

.bottomContent
{
	-moz-border-radius: 0 0 28px 28px; 
	-webkit-border-radius: 0 0 28px 28px;
	border-radius: 0 0 28px 28px;   
	height: 30px;
	behavior: url(/css/PIE.htc);
}

#plainarea {
	background-color: #ffffff;
	padding: 8px;
	overflow: hidden;
	zoom: 1;
	border-top: 1pt solid #cccccc;
}

#plainarea p {
	margin-top:18px;
}

#item {
	background-color: #ffffff;
	padding-top: 8px;
	padding-bottom: 8px;
	height: 80px;	
	overflow: hidden;
	zoom: 1;
	border-top: 1pt solid #cccccc;
	border-left: 1pt solid #cccccc;
	border-right: 1pt solid #cccccc;
}
#item.selected {
    background-color: #e7a486;	  
    height:120px;
}

#item.alt {
	background-color: #f0f0f0;
}

#item:hover {
	background-color: #E7A486; 
}

.text {
	height:auto;
	display: table;
	overflow: visible;
}

.text a {
	display: table-cell; 
	vertical-align: middle; 
	color: #000000;
	padding-right: 4px;
	font-size: 18px;
	text-decoration:none;
	font-weight:bold;
	height: inherit;
}
.text a.bigger {
    font-size: 22px;	
}
.text a:hover {
	display: table-cell; 
	vertical-align: middle;
	color: #000000;
	text-decoration:underline;
	font-weight:bold;
}

.text_list {
/*	line-height: 80px;*/
	height: 80px;
}

.rightnav .itemtext a {
	display:block;
	text-align:center
}

.image {
	width: 220px;
	height: 125px;
	-moz-border-radius: 6px;
	-webkit-border-radius: 6px;
/*	behavior: url(/css/ie-css3.htc);*/
	z-index:9;
}

.smallimage {
	width: 140px;
	height: 80px;
	-moz-border-radius: 6px;
	-webkit-border-radius: 6px;
	border-radius: 6px;
	margin-left: 12px;
	behavior: url(/css/ie-css3.htc);
	z-index:9;
}
.mediumimage {
	width: 160px;
	height: 120px;
	-moz-border-radius: 6px;
	-webkit-border-radius: 6px;
	border-radius: 6px;
	margin-left: 12px;
	behavior: url(/css/ie-css3.htc);
	z-index:9;
}

.largeimage {
	width: 300px;
	height: 170px;
	-moz-border-radius: 6px;
	-webkit-border-radius: 6px;
	border-radius: 6px;
	behavior: url(/css/ie-css3.htc);
	z-index:9;
}

#topnav {
	/* background: #ffffff;	*/
        /*padding:12px;*/
	-moz-border-radius: 28px; 
	-webkit-border-radius: 28px;
	border-radius: 28px;   
	text-align:center; 
	margin-top:5px;
	height:89px;
}

#tabs
{
	margin-left: 50px;
	overflow: hidden;
	margin-top: 12px;
	height: 50px;
}

#tabs table
{ 
	margin-bottom: -2px;
}

.tab
{
	padding-left:24px;
	padding-right:24px;
	padding-top:7px;
	padding-bottom:6px;
	margin-left:4px;
	font-family: Verdana;
	font-weight: bold;
	font-size: 14px;
	cursor:default;
	-moz-border-radius: 16px 16px 0 0; 
	-webkit-border-radius: 16px 16px 0 0;
	border-radius: 16px 16px 0 0;
	behavior: url(/css/PIE.htc);
}
.tab_small
{
	padding-left:7px;
	padding-right:7px;
	padding-top:7px;
	padding-bottom:7px;
	margin-left:4px;
	font-family: Verdana;
	font-weight: bold;
	font-size: 14px;
	cursor:default;
	-moz-border-radius-topleft: 8px; 
	-moz-border-radius-topright: 8px; 
	-webkit-top-border-left-radius: 8px;
	-webkit-top-border-right-radius: 8px;
	border-top-left-radius: 8px;   
	border-top-right-radius: 8px;
}

.tab_selected
{
	background:#DF6330;
	color:#ffffff;  
	text-decoration: none;
}

.tab_unselected
{
	color:#1e4164;
	background:#e6e6e6;
	text-decoration: none;
}

#content {
	overflow: hidden;
	zoom: 1;
}

#interior_content {
	border-left: 2px solid #CCCCCC;
	border-right: 2px solid #CCCCCC;
	background-color: #f7f7f7;
	padding: 20px;
}

#content_area p {
	margin-top:8px;
}

#content a { 
