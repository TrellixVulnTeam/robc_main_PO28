* {
	padding:0; margin:0; 
}

body {
	margin: 0;
	padding: 0;
	background-image:url("http://cdn.crowdignite.com/img/crowdignite/bg_gradient.png");
	background-repeat:repeat-x;
	background-position:left top;
	background-attachment:scroll;
	font-family: Helvetica, Arial, sans-serif;
}

h1, h2, h3, h4, h5, h5, p {
	margin: 0;
	padding: 0;
	font-family: Helvetica, Arial, sans-serif;
}

a {
   color: #ffffff; cursor: pointer
}

a:hover {
	text-decoration: none;
}

img {
	border: none;
}

ul {
    display:block;
    list-style-position:outside;
    margin:0;
    padding:0;
}

li {
	list-style-type: none;
}

#header {
	width: 1080px;
	margin-left: auto;
	margin-right: auto;
	overflow: hidden;
	height: 140px;
}

#top {
	height:auto;
	position:relative;
	/* background: #364a6f;*/
	-moz-border-radius-bottomleft: 28px; 
	-moz-border-radius-bottomright: 28px; 
	-webkit-border-bottom-left-radius: 28px;
	-webkit-border-bottom-right-radius: 28px;
	border-bottom-left-radius: 28px;   
	border-bottom-right-radius: 28px;   
}

#header .logo { 
	margin: 0 5px;
	overflow: hidden;
	float: left;
	display: inline;
}

#logo {
	margin-top:0px;
	margin-left:30px;
}

#tagline {
	text-shadow: 3px 1px 5px #111;	
	color:#1e4164;
	text-align:center;
	font-size:16px;
}
#banner_ad {
	position:relative;
	top:20px;
	left:26px;	
}
#other_stuff
{
	height:100px;
	text-align:right;	
}

#webmaster_area
{ 
	float:left;
	position:relative;
	top:17px;
	left:520px;
	width:230px;
	padding:8px;
	background-color:#DF6330;
	color:#ffffff;
	text-align:center;
	font-size:16px;
	-moz-border-radius: 12px; 
	-webkit-border-radius: 12px;
	border-radius: 12px;
	behavior: url(/css/PIE.htc);

}

#widget_button
{
	position:relative;
	top:17px;
	left:6px;
	width:230px;
	padding:8px;
	text-shadow: 3px 3px 7px #111;	
	color:#ffffff;
	text-align:center;
	font-size:16px;
	background: #d70d29;
	-moz-border-radius: 12px; 
	-webkit-border-radius: 12px;
	border-radius: 12px;  
}
#top_links
{
	color:#1e4164;
	margin-left:25px;	
	margin-top:25px;
}
#top_links a
{ 
	color:#1e4164;
}

#links
{
	margin-left:30px;
	height:30px;
	font-size:18px;
	color: #ffffff; 
	text-shadow: 3px 3px 7px #111;
}
table
{
   font-size:18px;	
}
input
{
    height: 24px;
    font-size:18px;    	
}

select
{
