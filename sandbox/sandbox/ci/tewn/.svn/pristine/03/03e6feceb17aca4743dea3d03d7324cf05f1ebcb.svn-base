@charset "utf-8";
.row { position:relative; overflow:visible; }  /* to position next button */
#btn_next { 
	top:180px; right:-10px;
	background:url(../img/widget_creation/btn_next.jpg) no-repeat;
	position:absolute;
	height:71px;
	width:49px;
}
#step_tabs {
	text-align:center;
	margin:15px 0 10px 0;
}
#step_tabs .tab {
	border:0;
	display:inline-block;
	font-family:Calibri, Arial, Helvetica, sans-serif;
	font-size:18px;
	font-weight:bold;
	height:50px;
	padding:10px 0px 10px 0;
	text-align:left;
	width:250px;
	cursor:pointer;
}
#step_tabs .tab div.nextarrow {
	background:url(../img/widget_creation/top_next_arrow.jpg) center right no-repeat;
	display:none;
	float:right;
	width:47px;
	height:47px;
}
#step_tabs #tab3 div.nextarrow { background:none; }
#step_tabs .tab a { color:#999; display:inline-block; float:left; width:100px; text-decoration:none; }
#step_tabs .tab a:hover { color:#a2a2a2; text-decoration:none; }
#step_tabs .tab .icon{
	display:block;
	float:left;
	background:url(../img/widget_creation/steps_sprite.jpg);
	height:	47px;
	width:	47px;
	margin:0 10px 0 0;
}
#step_tabs .selected a, #step_tabs .selected a:hover { color:#df6330; float:left; text-decoration:none; display:block; width:100px; }
#step_tabs #tab1 .icon{ background:url(../img/widget_creation/steps_sprite.jpg) no-repeat 0 0; }
#step_tabs #tab2 .icon{ background:url(../img/widget_creation/steps_sprite.jpg) no-repeat -48px 0; }
#step_tabs #tab3 .icon{ background:url(../img/widget_creation/steps_sprite.jpg) no-repeat -95px 0; }
#step_tabs #tab1.selected .icon{ background:url(../img/widget_creation/steps_sprite.jpg) no-repeat 0 -46px; }
#step_tabs #tab2.selected .icon{ background:url(../img/widget_creation/steps_sprite.jpg) no-repeat -48px -46px; }
#step_tabs #tab3.selected .icon{ background:url(../img/widget_creation/steps_sprite.jpg) no-repeat -95px -46px; }

#step_tabs .active a { color:#d55825; }
#wrapper { padding: 0 0 20px 0;}
#step_container {
	position:relative; 
	background:#d8d8d8;
	-webkit-border-radius: 10px;
	-khtml-border-radius: 10px;	
	-moz-border-radius: 10px;
	border-radius: 10px;
	margin:0 auto;
	overflow:hidden;
	width:96%;	
}

#white_arrow {
	top:0; left:140px;
	background:url(../img/widget_creation/step_arrow.jpg) no-repeat;
	display:block;
	height:15px;
	position:absolute;
	width:31px;
}

#step1 #white_arrow { left:127px; }
#step2 #white_arrow { left:393px; }
#step3 #white_arrow { left:662px; }

.step {	
	display:block;
	padding:0;
	margin:0 auto;
}

.step .step_row{ width:100%; padding:30px 0; }
.step .step_row.alt{ background:#d1d1d1; }

.step .step_row .name { 
	color:#888;
	display:inline-block;
	float:left;
	font-family: 'Arvo', Arial;
	font-size:24px;
	letter-spacing:-0.05em;
	padding:0 0 0 40px;
	text-shadow:1px 1px #FFFFFF;
	text-transform:uppercase; 
	width:300px;
}
.step .step_row .control {
	display:inline-block;
	margin: 0 20px 0 0;
	width:300px;
}
.step .step_row .fleft {float:left; } 
.step .step_row .fright {float:right; } 
.step .step_row .control.bot { padding:20px 0 0 0;}
.step .step_row .control #WidgetWebsiteId,
.step .step_row .control #widget_name{
	width:253px;
}

.step .step_row .control .colorbox {
	background-color:#333;
	-webkit-border-radius: 5px;
	-khtml-border-radius: 5px;	
	-moz-border-radius: 5px;
	border-radius: 5px;
	color:#fff;
	display:inline-block;
	font-weight:bold;
	font-size:14px;	
	height:25px;
	padding: 10px 10px 0 10px;	
	cursor:pointer;
	width:235px;
}
.step .step_row .control .colorbox span { 
	display:inline-block;
	float:left;
	height:14px;
}
.step .step_row .control .colorbox input{
	background:none;
	border:0;
	float:right;
	font-size:14px;
	font-weight:bold;	
	height:14px;
	text-align:right;
	width:90px;
}
.step .control .colorbox_clear_button {
	background:url(../img/widget_creation/reset_button.jpg) no-repeat top left;
	display:inline-block;
	height:17px;
	margin-left:3px;
	padding:0 0 8px 0;
	width:17px;
}

.crayonbox { display:none; padding:4px 2px; width:260px;}
.boxlid, .crayon {
	display:inline-block;
	color: #000;
	cursor: pointer;
	border: 1px solid #333;
	float: left;
	font-size:12px;
	font-family: Fixed, monospace;
	margin: 2px;
	text-align: center;
	height:15px;
	width: 15px;
}
#step1 .control { width:600px; text-align:center;}
#step1 .name{ padding-top:20px;}
#step1 .layout { color:#999; display:inline-block; height:80px; font-size:12px; font-weight:bold; text-transform:uppercase; margin:0 35px 0 0; }
#step1 .layout span { display:inline-block; width:100%; text-align:center; }
#step1 .layout a { 	display:inline-block;height:62px;}
#step1 .layout a:hover {-moz-opacity:.70; filter:alpha(opacity=70); opacity:.70; }
#step1 .layout.single a { background:url(../img/widget_creation/layout_sprite.jpg) no-repeat 0 -62px; width:60px; }
#step1 .layout.row a { background:url(../img/widget_creation/layout_sprite.jpg) no-repeat -95px -62px; width:80px; }
#step1 .layout.col a { background:url(../img/widget_creation/layout_sprite.jpg) no-repeat -221px -62px; width:35px; }
#step1 .layout.square a { background:url(../img/widget_creation/layout_sprite.jpg) no-repeat -314px -62px; width:59px; }
#step1 .layout.rectangle a { background:url(../img/widget_creation/layout_sprite.jpg) no-repeat -414px -62px; width:82px; }
#step1 .layout.single.selected a { background:url(../img/widget_creation/layout_sprite.jpg) no-repeat 0 0; }
#step1 .layout.row.selected a { background:url(../img/widget_creation/layout_sprite.jpg) no-repeat -95px 0; }
#step1 .layout.col.selected a { background:url(../img/widget_creation/layout_sprite.jpg) no-repeat -221px 0; }
#step1 .layout.square.selected a { background:url(../img/widget_creation/layout_sprite.jpg) no-repeat -314px 0; }
#step1 .layout.rectangle.selected a { background:url(../img/widget_creation/layout_sprite.jpg) no-repeat -414px 0; }
#step1 .layout.selected, #step1 .dimension.selected, #step1 .alignment.selected { color:#df6330; }
#step1 .dimension { color:#999; display:inline-block; height:100px; font-size:12px; font-weight:bold; text-transform:uppercase; margin:0 25px 0 0; }
#step1 .dimension span { display:inline-block; width:100%; text-align:center; }
#step1 .dimension a { 	display:inline-block;height:86px;}
#step1 .dimension a:hover {-moz-opacity:.70; filter:alpha(opacity=70); opacity:.70; }
#step1 .dimension.small a { background:url(../img/widget_creation/dimensions_sprite.jpg) no-repeat 0 -85px; width:56px; }
#step1 .dimension.medium a { background:url(../img/widget_creation/dimensions_sprite.jpg) no-repeat -80px -85px; width:75px; }
#step1 .dimension.large a { background:url(../img/widget_creation/dimensions_sprite.jpg) no-repeat -180px -85px; width:91px; }
#step1 .dimension.xlarge a { background:url(../img/widget_creation/dimensions_sprite.jpg) no-repeat -300px -85px; width:80px; }
#step1 .dimension.xxlarge a { background:url(../img/widget_creation/dimensions_sprite.jpg) no-repeat -410px -85px; width:94px; }
#step1 .dimension.small.selected a { background:url(../img/widget_creation/dimensions_sprite.jpg) no-repeat 0 0;  }
#step1 .dimension.medium.selected a { background:url(../img/widget_creation/dimensions_sprite.jpg) no-repeat -80px 0;  }
#step1 .dimension.large.selected a { background:url(../img/widget_creation/dimensions_sprite.jpg) no-repeat -180px 0; }
#step1 .dimension.xlarge.selected a { background:url(../img/widget_creation/dimensions_sprite.jpg) no-repeat -300px 0;  }
#step1 .dimension.xxlarge.selected a { background:url(../img/widget_creation/dimensions_sprite.jpg) no-repeat -410px 0;  }
#step1 .alignment { color:#999; display:inline-block; height:100px; font-size:12px; font-weight:bold; text-transform:uppercase; margin:0 55px 0 0; }
#step1 .alignment span { display:inline-block; width:100%; text-align:center; }
#step1 .alignment a { 	display:inline-block;height:85px; margin-bottom:5px;}
#step1 .alignment a:hover {-moz-opacity:.70; filter:alpha(opacity=70); opacity:.70; }
#step1 .alignment.center a { background:url(../img/widget_creation/alignment_sprite.jpg) no-repeat 0 0; width:60px; }
#step1 .alignment.left a { background:url(../img/widget_creation/alignment_sprite.jpg) no-repeat -80px 0; width:95px; }
#step1 .alignment.right a { background:url(../img/widget_creation/alignment_sprite.jpg) no-repeat -180px 0; width:95px; }
#step1 .alignment.center.selected a { background:url(../img/widget_creation/alignment_sprite.jpg) no-repeat 0 -86px;  }
#step1 .alignment.left.selected a { background:url(../img/widget_creation/alignment_sprite.jpg) no-repeat -80px -86px;  }
#step1 .alignment.right.selected a { background:url(../img/widget_creation/alignment_sprite.jpg) no-repeat -180px -86px; }

#step2 #cilink_desc {  color:#666; display:inline-block; font-size:12px; padding-right:100px;  width:200px;}
#step2 #cilink_label {  color:#333; display:inline-block; font-size:14px; font-weight:bold; margin:6px 0 0 10px; width:200px;}
#step2 div.spacemaker { display:inline-block; float:left; width:20px; height:20px; }
#step2 .padding_label,#step2 .padding_label_px { color:#333; float:left; font-size: 14px; font-weight: bold; margin:7px 20px 0 0;}
#step2 .padding_label_px { font-weight:normal; font-size: 12px; margin:7px 0 0 20px;}
#step2 .advanced { height: 30px; overflow:hidden; }
#step2 .advanced span { display:inline-block;  font-size:12px; float:left; margin:8px 15px; }
#step2 .advanced .control { color:#333; padding-bottom:20px; width:430px; }
#step2 .advanced .title { font-weight:bold; padding-top:10px; width:200px; }
#step2 .advanced .jqTransformSelectWrapper span { margin:0; }
#step2 .advanced .customcss { width:400px; height:100px; resize:vertical; }
#step2 .advanced span.button { background:url(../img/widget_creation/btn_advanced.jpg) no-repeat; display:inline-block; float:right; height:22px; margin:5px 400px 0 0; padding:0; width:171px; }
#step2 button>span {
    -webkit-border-radius: 3px;
    -khtml-border-radius: 3px;
    -moz-border-radius: 3px;
    border-radius: 3px;
}
#step2 #font_bold span span,
#step2 #font_italic span span,
#step2 #font_underline span span{
    height:24px;
    padding-top:9px;
}
#step2 .selected>span {
    background:#eee;
    border-top:1px solid #999;
    border-left:1px solid #999;
    height:32px;
}
#step2 .selected span span { background:#eee; padding-top:9px!important; height:23px!important;}
#step2 #font_bold span span { font-weight: bold; }
#step2 #font_italic span span { text-decoration: italic; }
#step2 #font_underline span span { text-decoration: underline; }

#step3 .control { text-align:right; width:620px; }
#step3 #get_code_button { background:url(../img/widget_creation/btn_getcode.jpg) no-repeat; display:inline-block; height:79px; margin-right:170px; width:272px; }
#step3 #get_code_button.save { background:url(../img/widget_creation/btn_savechanges.jpg) no-repeat; }
#step3 #goto_dashboard_button { background:url(../img/widget_creation/btn_goto_dashboard.jpg) 0 2px no-repeat; display:inline-block; height:79px; width:157px; }
#step3 #widget_code { float:left; width:400px; height:100px;}
#step3 .columnr { width:300px; }
#step3 #widget_code_instructions { color:#666; display:inline-block; font-size:12px; margin:5px 20px 0 40px; }
#step3 #preloader {
    background:url(../img/widget_creation/widget_ajax_loader.gif) no-repeat;
    color: #666666;
    display:block;
    font-size: 16px;
    height: 24px;
    margin: 30px auto 0;
    padding-left: 40px;
    text-align: left;
    width: 550px;
}
#widget_preview {
	background:url(../img/widget_creation/preview_stripes.png) repeat;
	display:block;
	padding:30px 0;
	position:relative;
	text-align:center;
}
#widget_preview #widget_size {
	color:#666;
	display:inline-block;
	font-weight:bold;
	font-size:14px;
	left:20px;	
	position:absolute;
	text-align:left;	
	top:20px;	
	width:130px;
}
#widget_preview #widget_size span{ color:#333; }
#your_widget { display:block; margin:10px auto 0 auto; padding:2px; overflow:hidden; }
#cilinkdiv { display:block; margin: 0 auto 10px auto; padding:2px; }
#your_widget img { background:#fff; display:inline-block; }
#your_widget.aligned table {
	border-spacing:0;
	display:block;
	table-layout:fixed;
}
#your_widget.aligned table tr {
        width:100%;
	display:inline-block;
	float:left;
	margin:0;
	padding:0;
}
#your_widget.aligned table tr td {
/* min-width:230px; */
	display:block;
	float:left;
	margin:0;
	padding:5px 0 0 5px;
	text-align:left;
}
#your_widget.aligned table tr td img {
        float:left;
	margin-right:5px;
}
#your_widget.aligned table tr td>div {
        margin:10px 0 0 5px;
        float:left;
	clear:none!important;
	display:block;	
}

/* landingpage creator preview*/
#lp_preview {
	background:url(../img/widget_creation/preview_stripes.png) repeat;
	display:block;
        margin:20px 0 0 0;
	padding:0;
	position:relative;
	text-align:left;
}
#lp_preview #lp_wrap{
    width:900px;
    margin:0 auto;
    padding:0 0 30px 0;
}
#lp_preview #lp_wrap div#logo{
    text-align: left;
    margin:20px 0;
}
#lp_preview #lp_wrap .item {
    border:1px solid #ccc;
    background:#fff;
    display:inline-block;
    width:180px;
    height:180px;
    padding:10px;
    margin:10px 10px 0 10px;
    -moz-border-radius: 4px; /* Firefox */
    -webkit-border-radius: 4px; /* Safari, Chrome */
    border-radius:4px; /* CSS3 */
}
#lp_preview #lp_wrap .item h2 {
    display:block;
    padding:8px 0;
    font-size:12px;
}
#lp_preview #lp_wrap .item img {
    width:176px;
    height:120px;
    border:1px solid #ccc;
}
.prelabel { margin-left:-50px!important;}
.wider_input, .wider_input div {
    width:270px!important;
}
.wider_input input {
    width:255px!important;
}

#flashMessage {
    margin-left:0;
    padding: 5px;
    width: 100%;
    text-align:center;
}
.columnr { float:left; width:320px; }
.clear { clear:both; }