body{
	background:url(/img/thecelebritycafe/header-background2.png) 0 -170px repeat-x;
	margin:0;
	padding:0;
}
#wrap {	background: transparent!important; margin:0 auto;}
#header {
        background: transparent!important;
	height:170px;
	position:relative;
}
#header .logo{
	background: url(/img/thecelebritycafe/logo.png) repeat-x;
	height:65px;
	left:0;
	position:absolute;
	top:30px;
	width:400px;
}
#nav { display:block; left:0; position:absolute; top:106px; }
#nav ul { list-style: none outside none; margin: 0; padding: 0;}
#nav ul li { display:inline-block; margin:0 ; padding:0; float:left;  height: 45px;}
#nav ul li a {
    color: #9FDCEA;
    display: inline-block;
    font-family: Century Gothic,'Trebuchet MS',Helvetica,sans-serif;
    font-size: 1.05em;
    height: 45px;
    line-height: 45px;
    margin: 0;
    overflow: hidden;
    padding: 0 25px;
    text-align: center;
    text-decoration: none;
    text-transform: uppercase;
    white-space: nowrap;
}
#nav ul li a:hover { background:#032836; color:#fff; }
#footer { background:#102831; height:150px; margin:20px 0 0; width:100%; }
#footer div.innerfooter { font-size:16px; margin:0 auto; text-align:center; width:960px;}
#footer ul.menu { width:700px; display:inline-block; font-family:Arial, Helvetica, sans-serif; margin:20px auto; padding:0; }
#footer ul.menu li { display:inline-block; float:left; list-style:none; margin:0 20px; padding:0; }
#footer ul.menu li a{ display:inline-block; color:#fff; text-decoration:none; }
#footer ul.menu li a:hover{ border-bottom:1px solid #718d97; }
.clearfloat { clear:both; height:0; margin:0; padding:0; }

/* signup */
div.main #content form div.submit input{
    background: url(/img/thecelebritycafe/btnsignup.png) no-repeat;
    border:none;
    height: 69px;
    margin-top:10px;
    text-indent: -9999px;
    width:282px;
}
div.main #content form div.submit input:hover {
    background-position:0 -68px;
}
div.row { width:960px; margin:0 auto; }
div.row #content { background: #FFF; margin:0 auto; padding: 0 10px 10px 10px; width: 942px; }
.grid_12 .grid_3 { margin-right:5px; }
div.row #content ul {
    background:#fff;
    -moz-border-radius: 10px; /* Firefox */
    -webkit-border-radius: 10px; /* Safari, Chrome */
    border-radius: 10px; /* CSS3 */
}
h3.topContent { display: none; }
div.row #content ul, div.main  #content li { list-style: none;  }
div.row #content .column { width:920px; }
div.row #content .column  div#footer { border:0; background:none!important; }
div.submit { padding-left:125px; text-align:left; }
div.submit input:hover {
    background-position:0 -68px;
}
#create_form ul li.last {
    height: 7em!important;
    padding-bottom:2em!important;

}
div.row #content ul {
    background:#fff;
    box-shadow:0 1px 3px #999999;
    -moz-border-radius: 3px; /* Firefox */
    -webkit-border-radius: 3px; /* Safari, Chrome */
    border-radius:3px; /* CSS3 */
}
/* signup 2 part */
#create_form {  position:relative;}
#create_form ul { padding-top:20px; }
#create_form .title {
    width:95%;
    margin:10px auto 10px;
    padding:10px 0;
    /*border-bottom:1px solid #ccc;*/
}
#create_form .title h2 {
    background:url(/img/crowdignite/ci_fire_icon.png) no-repeat 0 5px;
    color:#333;
    display:block;
    margin:0;
    padding:0 0 0 25px;
    font-size:27px;
    letter-spacing: -0.05em;
}
#create_form .title p {
    color:#666;
    font-size:14px;
    line-height: 1em;
    margin:2px 0 10px 0;
    padding-left:25px;
    text-align:left;
}
#create_form div.control {
    position:relative;
    display:block;
    float:left;
    line-height:2em;
    margin:0;
    padding:0;
}
#create_form label.error {
    background:#FFF2BF url(/img/crowdignite/icon_deny.png) no-repeat 5px 5px;
    border:1px solid #e5d595;
    color:#900;
    display:block;
    font-size:14px;
    line-height:1.1em;
    margin:0;
    padding:6px 0 6px 29px;
    position: absolute;
    text-align:left;
    top: 0;
    left:280px;
    width: 350px;
    z-index: 100;
}
#create_form ul {
    width: 100%; min-height:400px;
}
#create_form ul li {
    width:95%;
    margin:0 auto;
}
#create_form ul.form>li {
    height:3em;
    padding:1em 0 0.2em 0;
}
#create_form ul.form>li>label {
    color:#333;
    float:left;
    line-height:2em;
    margin:0;
    padding:0 30px 0 0;
    text-align:right;
    width:120px;
}
#create_form ul.form li span.terms {
    color:#333;
    font-size:14px;
    display: inline-block;
    padding-left:8px;
}
#create_form ul.form li label.terms a { color:#333!important; text-decoration: underline; }
#create_form ul li.alt {
    background:#fff;
}
#create_form ul li.focus_line {
    background:#fdf7df;
}
#create_form div.join_text {
    position:absolute;
    right:30px;
    text-align:left;
    top:120px;
    width:380px;
}
#create_form div.join_text #img_what_is_ci {
    background: url("/img/crowdignite/what_is_ci.png") no-repeat;
    display: inline-block;
    height: 205px;
    margin:10px -10px 0 0;
    width: 374px;
}
#create_form div.join_text h2 {
    color:#E26832;
    font-size: 25px;
    letter-spacing:-0.03em;
    padding:0;
}
#create_form div.join_text p {
    color:#333;
    font-size:14px;
    width:90%;
}
#create_form div.join_text p a {
    color:#E26832;
    text-decoration:none;
}
/* Eo signup */
.grid_12 {
    margin-left: 0;
}

#content a {
    color: #000000;
}
.text {
    display: table;
    height: auto;
    overflow: visible;
}
.text a {
    color: #000000;
    display: table-cell;
    font-size: 18px;
    font-weight: bold;
    height: inherit;
    padding-right: 4px;
    text-decoration: none;
    vertical-align: middle;
}