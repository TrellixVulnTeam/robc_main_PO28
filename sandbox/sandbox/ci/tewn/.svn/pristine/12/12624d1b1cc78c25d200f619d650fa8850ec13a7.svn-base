<style>
@font-face {
    font-family: 'LeagueGothicRegular';
    src: url('/fonts/league_gothic-webfont-webfont.eot');
    src: url('/fonts/league_gothic-webfont-webfont.eot?#iefix') format('embedded-opentype'),
         url('/fonts/league_gothic-webfont-webfont.woff') format('woff'),
         url('/fonts/league_gothic-webfont-webfont.ttf') format('truetype'),
         url('/fonts/league_gothic-webfont-webfont.svg#LeagueGothicRegular') format('svg');
    font-weight: normal;
    font-style: normal;
}

ul.navigation {
	margin:110px auto 0 auto;
	padding:0 40px 0 0;
	position:relative;
}

.top_content_spacer { display:inline-block; margin:0;}

#content, .bottom_content_spacer { float:none; margin-left:0!important; margin-right:0!important; display:block; }
div#page_body {	background:none; width:80%; min-width:900px; }
#interior_content, .container_12 .grid_12 { width:100%;  float:none; border:0; padding:0; /*width: 1080px;*/}

#links_content {
	border-radius:0;
	float:none;
	clear:both;
	display:block;
	-moz-border-radius:0;
	-webkit-border-radius:0;
	margin:0 auto!important;
	padding:0;
}
#paginate { padding-top:10px; margin:0;  }
/* small_grid */
.grid_7,.grid_5 { float:none; margin:0; }

.container_12 .grid_5,
.container_12 .grid_7 > .grid_7,
.container_12 .grid_5 > .grid_5 {
	width: auto;
	margin:0;
}
#links_content {
    /* width:1003px; */
}
.grid_12 .grid_5 img {
	border-radius:0;
	-moz-border-radius:0;
	-webkit-border-radius:0;
}
.bottom_content_spacer { margin-bottom:15px; }
.grid_12>.grid_7 {
    height:auto;
    width:auto;
    display:inline;
}

/* font loader styles */
.wf-loading .item div.text p a {
	visibility:hidden;
}
.wf-active .item div.text p a {
	font-family:"LeagueGothicRegular", Arial, sans-serif;
	font-size:24px!important;
	font-weight:normal;
	line-height:30px!important;
}

/* all boxes */
.item {
        background-color: #FFFFFF;
        border-radius: 5px;
        -moz-border-radius: 5px;
        -webkit-border-radius: 5px;
        box-shadow: 0 1px 3px #CCC;
        -moz-box-shadow: 1px 1px 3px #999999;
        -webkit-box-shadow: 1px 1px 3px #999999;
        float:left;
        margin:9px 9px!important;
        overflow: hidden;
        padding: 10px;
        cursor: pointer;
		text-align:left;
        width: 190px;
}
.item:hover {
        background-color: #E6E6E6;
        /*border: 1px solid #B3B3B3;*/
}
.item .image {
        border-radius:2px!important;
        -moz-border-radius:2px!important;
        -webkit-border-radius:2px!important;
        display: block;
        height: 105px;
        overflow: hidden;
        width: 190px;
}
.item .image a {
        display:block;
}
.item .image img{
        border:0;
        height:auto; /* 145px; */
        width:210px;
}
.item div.text p{
        color: #209E9C;
        font-size: 45px;
        font-weight: 700;
		letter-spacing:0em;
		line-height:0.6em;
        margin:4px 0 0 0;
		text-transform:uppercase;
}
.item div.text p a{
        border:0;
        color: #555!important;
        display: block;
		font-size:20px;
		font-weight:normal;
		line-height:25px;
		letter-spacing: 0;
        max-width: 190px;
        overflow: hidden;
        text-decoration: none;
        width:180px;
}
.item:hover div.text p a { color:#df6330!important; }
/* double item */
.grid_12 .double_item {
        height: auto!important;
        width: 190px;
        display:block;
}
.grid_12 double_item div.text p {
        font-size: 22px !important;
        line-height: 24px !important;
}
.grid_12 .double_item .image img {
        border:0;
        width:190px!important;
        height:auto;
}
.grid_12 .double_item .image,
.grid_12 .double_item .image a  {
        width: 190px!important;
        height: auto!important;
}
.grid_12 .double_item div.text {
    width:190px;
}
.grid_12 .double_item div.text p a {
	border:0;
	color: #333;
	font-weight:normal;
	max-width: 180px;
	overflow: hidden;
	padding-bottom:5px;
	padding-left:5px;
	text-decoration: none;
	width:180px;
}

/* large items */
.grid_12 .large_item {
        height: 324px;
        width: 420px;
        display:block;
}
.grid_12 large_item div.text p {
        font-size: 22px !important;
        line-height: 24px !important;
}
.grid_12 .large_item .image img {
        border:0;
        width:420px!important;
        height:auto;
}
.grid_12 .large_item .image,
.grid_12 .large_item .image a  {
        width: 420px!important;
        height: 250px!important;
}
.grid_12 .large_item div.text {
    width:416px;
}
.grid_12 .large_item div.text p a {
	border:0;
	color: #333;
	font-size: 40px!important;
	font-weight:normal;
	line-height: 24px!important;
	line-height:0.9em!important;
	max-width: 410px;
	overflow: hidden;
	padding-bottom:5px;
	padding-left:5px;
	text-decoration: none;
	width:410px;
}
.bottom_content_spacer { margin-bottom:15px; }
.grid_12>.grid_7 {
    height:auto;
    width:auto;
    display:inline;
}
.grid_12 .item:last-child { clear:both; }
</style>

