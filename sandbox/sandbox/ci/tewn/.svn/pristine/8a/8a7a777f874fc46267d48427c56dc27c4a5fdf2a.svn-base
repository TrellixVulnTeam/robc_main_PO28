/*dropdowns*/
.ddown_container { display: inline-block; padding:5px 0 5px 5px;}
.ddown_container3 { display: none; }
.dropdown { font-size:12px; display:inline-block; margin:0 10px; white-space: nowrap; }
.dropdown dd, .dropdown dt, .dropdown ul { margin:0px; padding:0px; }
.dropdown dd { position:relative; }
.dropdown a, .dropdown a:visited { color:#666; text-decoration:none; outline:none;}
.dropdown a:hover { color:#333;}
.dropdown dt a:hover, .dropdown dt a:focus { color:#5d4617; border: 1px solid #bdbdbd;}
.dropdown dt a {color:#333!important; background:#eee url(/img/crowdignite/arrow_down.png) no-repeat scroll 145px center!important; display:block; padding-right:20px;
                border:1px solid #bdbdbd; width:150px;
	-webkit-border-radius: 3px;
	-moz-border-radius: 3px;
	border-radius: 3px;
}
.dropdown dt a span {cursor:pointer; display:block; padding:5px; font-weight:normal!important;}
.dropdown dd ul {
  background:#eee none repeat scroll 0 0; border:1px solid #bdbdbd; color:#666; display:none;
  left:0px; padding:5px 0px; position:absolute; top:2px; width:auto; min-width:170px; list-style:none;
	-webkit-border-radius: 3px;
	-moz-border-radius: 3px;
	border-radius: 3px;
    box-shadow: 1px 2px 3px rgba(0, 0, 0, 0.48);
    -moz-box-shadow: 1px 2px 3px rgba(0, 0, 0, 0.48);
    -webkit-box-shadow: 1px 2px 3px rgba(0, 0, 0, 0.48);
  z-index: 999;
}
.dropdown span.value { display:none; font-weight:normal!important;}

.dropdown dd ul li a { padding:5px; display:block; font-weight:normal!important;}
.dropdown dd ul li a:hover { background-color:#ddd; color:#444!important;}

.dropdown dd ul li.tab_selected a { background:#bbb; color:#333; padding:5px; display:block;}
.dropdown dd ul li.tab_selected a:hover { background-color:#aaa; color:#222!important;}
.dropdown dd ul li input { margin:0 5px 0 10px; }

.dropdown img.flag { border:none; vertical-align:middle; margin-left:10px; }
.flagvisibility { display:none;}
/* end dropdown*/

