class WidgetAnalyticTestCase extends CakeTestCase {
	var $WidgetAnalytic = null;
	var $fixtures = array('app.widget_analytic');

	function startTest() {
		$this->WidgetAnalytic =& ClassRegistry::init('WidgetAnalytic');
	}

	function testWidgetAnalyticInstance() {
		$this->assertTrue(is_a($this->WidgetAnalytic, 'WidgetAnalytic'));
	}

	function testWidgetAnalyticFind() {
		$this->WidgetAnalytic->recursive = -1;
		$results = $this->WidgetAnalytic->find('first');
		$this->assertTrue(!empty($results));

		$expected = array('WidgetAnalytic' => array(
			'id' => 1,
			'domain_bitfield' => 1,
			'page_id' => 6,
			'shown_website_id' => 2,
			'num_ctr' => 5,
			'num_display' => 120,
			'num_ctr_traffic' => 0,
			'weight' => 1,
			'created' => '2010-03-06 17:11:44',
			'visible' => 1,
			'type' => 0,
			'key' => 0
		));

		$this->assertEqual($results, $expected);
	}
}
