<script id="ci_stats_full_system_year" type="text/x-handlebars-template">
    <thead>
        <tr>
            <th class="text-center" colspan="2">BI MONTHLY DATA</th>
            <th class="text-center">All Verticals RETURN RATE</th>
            <th class="text-center">Crave RR</th>
            <th class="text-center">GameRev</th>
            <th class="text-center">TotallyHer RR</th>
            <th class="text-center">Momtastic RR</th>
            <th class="text-center">TFS RR</th>
            <th class="text-center">CTR</th>
        </tr>
    </thead>
    <tbody>
    {{#each ci_stats_full_system_year}}
        <tr>
            <td class="text-center" rowspan="{{total_chunks}}"> {{@key}}</td>
            {{#each this}}
            {{#if craveonline.return_rate}}
            <td class="text-center text-nowrap">{{ chunk }}</td>
            <td class="text-right">{{ return_rate }}%</td>
            <td class="text-right">{{ craveonline.return_rate }}%</td>
            <td class="text-right">{{ gamerevolution.return_rate }}%</td>
            <td class="text-right">{{ totallyher.return_rate }}%</td>
            <td class="text-right">{{ momtastic.return_rate }}%</td>
            <td class="text-right">{{ thefashionspot.return_rate }}%</td>
            <td class="text-right">{{ ctr }}%</td>
            </tr>
            {{/if}}
            {{/each }}
    {{/each}}
    </tbody>
</script>