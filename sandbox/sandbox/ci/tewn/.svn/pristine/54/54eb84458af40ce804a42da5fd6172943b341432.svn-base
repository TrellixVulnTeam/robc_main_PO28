<script type="text/x-handlebars-template" id="subcategory-row">
    <tr id="subcategory-{{id}}" class="success">
        <td class="text-right">{{id}}</td>
        <td class="name">{{name}}</td>
        <td>{{category}}</td>
        <td>{{created}}</td>
        <td class="text-center">
            <button type="button" class="btn btn-default btn-xs" data-toggle="modal" data-target="#edit-subcategory" data-id="{{id}}" data-name="{{name}}">
                <span class="fa fa-edit"></span>
            </button>
        </td>
    </tr>
</script>