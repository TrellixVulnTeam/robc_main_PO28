class HistoryTimerTestCase extends CakeTestCase {
	var $HistoryTimer = null;
	var $fixtures = array('app.history_timer');

	function startTest() {
		$this->HistoryTimer =& ClassRegistry::init('HistoryTimer');
	}

	function testHistoryTimerInstance() {
		$this->assertTrue(is_a($this->HistoryTimer, 'HistoryTimer'));
	}

	function testHistoryTimerFind() {
		$this->HistoryTimer->recursive = -1;
		$results = $this->HistoryTimer->find('first');
		$this->assertTrue(!empty($results));

		$expected = array('HistoryTimer' => array(
			'id' => 1,
			'type' => 1,
			'time' => 1,
			'data' => 'Lorem ipsum dolor sit amet',
			'created' => '2010-08-22 14:28:35'
		));
		$this->assertEqual($results, $expected);
	}
}
