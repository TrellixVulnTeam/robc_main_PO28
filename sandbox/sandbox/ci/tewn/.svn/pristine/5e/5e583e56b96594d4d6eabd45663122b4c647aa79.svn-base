    background: transparent url(/img/analysis/quad-lr.png) center center no-repeat;
    width: 16px;
    height: 16px;
}

.glyphicon-quad-ur {
    background: transparent url(/img/analysis/quad-ur.png) center center no-repeat;
    width: 16px;
    height: 16px;
