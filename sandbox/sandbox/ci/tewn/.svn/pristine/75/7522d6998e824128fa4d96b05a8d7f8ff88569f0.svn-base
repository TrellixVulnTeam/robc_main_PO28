<script id="ci_stats_by_hub" type="text/x-handlebars-template">
    <thead>
        <tr>
            <th class="text-center">Date</th>
            <th class="text-center">Hub Name</th>
            <th class="text-center">CTR</th>
            <th class="text-center">Return Rate</th>
            <th class="text-center">Widget Impressions</th>
            <th class="text-center">TPM</th>
            <th class="text-center">Gen RR</th>
            <th class="text-center">Traffic From</th>
            <th class="text-center">Traffic To</th>
            <th class="text-center">Credits</th>
            <th class="text-center">Imp. Basic</th>
            <th class="text-center">Imp Similar</th>
            <th class="text-center">Imp Contextual</th>
            <th class="text-center">CTR Basic</th>
            <th class="text-center">CTR Similar</th>
            <th class="text-center">CTR Contextual</th>
        </tr>
    </thead>
    <tbody>
    {{#each ci_stats_by_hub}}
        <tr>
            <td class="text-right text-nowrap">{{ date }}</td>
            <td class="text-center">{{ hub_name }}</td>
            <td class="text-right">{{ ctr }}%</td>
            <td class="text-right">{{ return_rate }}%</td>
            <td class="text-right">{{ numberFormat widget_impressions }}</td>
            <td class="text-right">{{ tpm }}</td>
            <td class="text-right">{{ gen_rr }}%</td>
            <td class="text-right">{{ numberFormat traffic_from }}</td>
            <td class="text-right">{{ numberFormat traffic_to }}</td>
            <td class="text-right">{{ numberFormat credits }}</td>
            <td class="text-right">{{ numberFormat imp_basic }}</td>
            <td class="text-right">{{ numberFormat imp_similar }}</td>
            <td class="text-right">{{ numberFormat imp_contextual }}</td>
            <td class="text-right">{{ ctr_basic }}%</td>
            <td class="text-right">{{ ctr_similar }}%</td>
            <td class="text-right">{{ ctr_contextual }}%</td>
        </tr>
    {{/each}}
    </tbody>
</script>