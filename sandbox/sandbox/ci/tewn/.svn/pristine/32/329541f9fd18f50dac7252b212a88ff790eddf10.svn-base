<script type="text/javascript">
	// This is our refresh event, it fires on dom load, window load, and window resize as well as when any widget changes the page dimensions
	$(document).ready(function()	{ $(document).trigger('refresh') })
	$(window).load(function()		{ $(document).trigger('refresh') })
	$(window).resize(function()		{ $(document).trigger('refresh') })

	// Bind placeFooter to our refresh event 
	$(document).bind('refresh', function() { placeFooter() })

    function placeFooter(padding) {
        var footer = '#global-footer_0'

        //Short circuit if we're missing our footer
        if ($(footer).length < 1) return
        
        // If the wrapper's hidden, we're being blocked by an interstitial
        if ($("#wrapper").css('display') == 'none') {
            // Set an interval for later and exit
            sbFooterTimeout = window.setTimeout(placeFooter, 1000)
            return false
        }

		// Set a default padding
        if (!padding) padding = 2

        var top, height,footer_height,footer_top
        var viewport_height = $(window).height()
		var max = 0
		var ignorables = $('.placeFooter_ignore') // This is our mechanism for ignoring elements
		footer_top = $(footer).offset()['top']  // Capture the current footer offset from the top of the page
		footer_height = $(footer).outerHeight()

		// Grab ever div under #wrapper that isn't our footer or in our "ignorables" group
		$('#wrapper > div').not(footer).not(ignorables).each(function() {
			// Have jQuery grab the div's position and height
			top = $(this).offset()['top']
			height = $(this).outerHeight()

			// Test the current max height and set our new max if needed
			if ((height + top) > max) {
				max = height + top + padding
			}
		})

		// If our combined footer height and offset is greater that the viewport, stick it at the bottom of the page
		if ((footer_height + max) > viewport_height) {
			$(footer).css({ 
				top: max + 'px',
				bottom: 'auto'
			})

			// Rejigger out wrapper height to wrap around the entire page contents
			$('#wrapper').css({ height: (footer_height + max) + 'px' })
		// Otherwise tack the footer on the bottom of the viewport
		} else {
			$(footer).css({
				top: 'auto',
				bottom: '0'
			})
			
			// Remove wrapper's height property, and rely on minHeight:100%
			$('#wrapper').css({ height: '' })
		}

		// Display the footer
		$(footer).css({
			zIndex: '0',
			display: 'block'
		})
	}
</script>

