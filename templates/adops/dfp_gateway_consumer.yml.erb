---
:worker_manager: 'ApDfpApi::WorkerManager'
:configs:
  :dfp:       'ApDfpApi::Config::Dfp'
  :reporting: 'ApDfpApi::Config::Reporting'

:prefetch: 1
:no_ack: true
:exclusive: false

:exchange: adplatform
:exchange_options:
  :type: :direct
  :durable: true
  :auto_delete: false
  :exclusive: false

:routing_key: dfp
:queue: dfp
:queue_options:
  :durable: true
  :auto_delete: false
  :exclusive: false

:content_type: application/json
:content_encoding: utf-8
