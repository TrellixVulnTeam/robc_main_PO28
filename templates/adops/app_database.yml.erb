<%= app_env_name %>:
  adapter: mysql2
  database: <%= app_db_name %>
  username: <%= app_db_user %>
  password: <%= app_db_pass %>
  host: <%= app_db_host %>

<%= app_env_name %>_migration:
  adapter: mysql2
  database: <%= app_db_name %>
  username: <%= app_db_migration_user %>
  password: <%= app_db_migration_pass %>
  host: <%= app_db_host %>
