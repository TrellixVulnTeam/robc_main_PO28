
<VirtualHost *:80>
    ServerName wholesomebabyfood.com
    ServerAlias www.wholesomebabyfood.com
    DocumentRoot /app/shared/docroot/

        <Directory /app/shared/docroot/>
                Options Indexes FollowSymLinks
                Order allow,deny
                Allow from all
        </Directory>


