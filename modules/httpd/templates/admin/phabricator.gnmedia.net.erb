<VirtualHost *:80>

    ServerName phabricator.gnmedia.net
    <%=logging ? '' : '#'%>CustomLog /app/log/phabricator.gnmedia.net-access.log combined
    <%=logging ? '' : '#'%>ErrorLog /app/log/phabricator.gnmedia.net-error.log

    DocumentRoot /app/shared/docroots/phabricator.gnmedia.net/phabricator/webroot/

    RewriteEngine on
    RewriteRule ^/rsrc/(.*)     -                       [L,QSA]
    RewriteRule ^/favicon.ico   -                       [L,QSA]
    RewriteRule ^(.*)$          /index.php?__path__=$1  [B,L,QSA]

</VirtualHost>
