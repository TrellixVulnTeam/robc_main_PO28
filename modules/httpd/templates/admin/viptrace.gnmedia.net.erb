<VirtualHost *:80>
    ServerName viptrace.gnmedia.net
    DocumentRoot /app/shared/docroots/viptrace.gnmedia.net/

    <%=logging ? '' : '#'%>ErrorLog /app/log/viptrace.gnmedia.net_error.log
    <%=logging ? '' : '#'%>CustomLog /app/log/viptrace.gnmedia.net_access.log combined

    <Directory /app/shared/docroots> 
        Options FollowSymLinks
        AllowOverride All
        Satisfy all
    </Directory>

</VirtualHost>
