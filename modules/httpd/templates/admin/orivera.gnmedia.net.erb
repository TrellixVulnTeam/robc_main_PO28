<VirtualHost *:80>
    ServerName orivera.gnmedia.net
    #ServerAlias *.tp.dev.lax.gnmedia.net
    DocumentRoot /app/shared/docroots/orivera.gnmedia.net/

    <%=logging ? '' : '#'%>ErrorLog /app/log/orivera.gnmedia.net_error.log
    <%=logging ? '' : '#'%>CustomLog /app/log/orivera.gnmedia.net_access.log combined

    <Directory /app/shared/docroots> 
        Options FollowSymLinks
        AllowOverride All
        Satisfy all
    </Directory>
</VirtualHost>
