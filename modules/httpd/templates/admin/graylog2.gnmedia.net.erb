# File managed by puppet
# Do not edit it on <%= fqdn %>

<VirtualHost *:80>
    ServerName  graylog2.gnmedia.net
    ServerAlias *.graylog2.gnmedia.net
    DocumentRoot /app/shared/graylog2/web/public
    <%=logging ? '' : '#'%>ErrorLog /app/log/error.log
    <%=logging ? '' : '#'%>CustomLog /app/log/access.log combined

    <Directory /app/shared/graylog2/web/public>
      Allow from all
      Options -MultiViews
    </Directory>

</VirtualHost>

