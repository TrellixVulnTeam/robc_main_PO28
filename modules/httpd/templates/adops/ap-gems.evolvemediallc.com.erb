# This file is managed by Puppet. Do not modify it from <%=fqdn %>
# vim: set ft=apache :

<VirtualHost *:80>
    ServerName ap-gems.evolvemediallc.com
    DocumentRoot "/app/software/ruby/geminabox/public"
    Timeout 600

    <%=logging ? '' : '#'%>ErrorLog /app/log/gems.evolvemediallc.com.error.log
    LogLevel warn
    <%=logging ? '' : '#'%>CustomLog /app/log/gems.evolvemediallc.com.access.log combined

    PassengerAppEnv production
    <Directory /app/software/ruby/geminabox/public>
      PassengerRuby /app/software/ruby/rbenv/versions/2.1.1/bin/ruby
      Allow from all
      Options -MultiViews
    </Directory>
</VirtualHost>
