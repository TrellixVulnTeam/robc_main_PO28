# <%= name %> - generated by puppet
# /etc/httpd/conf.d/<%= name %>.conf
<VirtualHost *:80>
    ServerName <%= name %>
    ServerAlias *.<%= name %>
    ErrorLog /app/log/<%= name %>-error.log
    CustomLog /app/log/<%= name %>-access.log combined

    ProxyPass / http://127.0.0.1:<%= nodeport %>/
    ProxyPassReverse / http://127.0.0.1:<%= nodeport %>/
</VirtualHost>

